@article{ WOS:000905702500005,
Author = {Kopelev, D. N.},
Title = {``Pirates Will More Pleasantly Accept the Patronage of His Imperial
   Majesty{''}: Colonial Projects of Peter the Great and Jacobite
   Conspiracies (1718-1725)},
Journal = {HERALD OF THE RUSSIAN ACADEMY OF SCIENCES},
Year = {2022},
Volume = {92},
Number = {SUPPL 8, 8, SI},
Pages = {S703-S712},
Month = {DEC},
Abstract = {This article is devoted to the issues of the colonial policy of Peter
   the Great in 1718-1725. Drawing on new archival documents, the author
   analyzes the trends in the geopolitical development of the Russian
   Empire in the focus of the ideology of the Jacobite movement and
   connects the main trends of Russia's imperial expansion in the World
   Ocean with the plans for the restoration of the Stuart dynasty,
   interpreting in a new way the geopolitical plans of Peter the Great
   regarding an alliance with the pirates of Madagascar and the
   organization of slave trade.},
