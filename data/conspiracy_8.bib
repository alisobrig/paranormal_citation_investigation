@article{ WOS:000941316800001,
Author = {Milosavljevic, Milos and Okanovic, Milan and Cicvaric Kostic, Slavica
   and Jovanovic, Marija and Radonic, Milenko},
Title = {COVID-19 and Behavioral Factors of e-Payment Use: Evidence from Serbia},
Journal = {SUSTAINABILITY},
Year = {2023},
Volume = {15},
Number = {4},
Month = {FEB},
Abstract = {Banknotes and coins are some of the most frequently traded items in the
   world. Their current use, however, is unsustainable, and many countries
   are trying to digitalize their payment systems. The recent pandemic has
   accelerated this transition. Building on the Theory of Unintended
   Consequences, the aim of this article is to examine the influence of
   some pandemic-specific factors (in specific, hand sanitization,
   conspiracy theory mentality, and financial acumen) on the current and
   prospective use of e-payment. A particular aim of the study is to
   analyze these relationships in Serbia (as an example of a cash-centric
   society). The study is based on primary data gathered via a
   questionnaire. The questionnaire was designed for the purpose of this
   study. In total, the study examined N = 474 examinees. The results of
   this study confirm that the pandemic-induced variables are statistically
   significant predictors of e-payment use. In particular, hand
   sanitization, conspiracy mentality (reversely), and financial acumen
   positively affect current and prospective e-payment use.},
