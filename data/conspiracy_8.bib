@article{ WOS:000449602700005,
Author = {Buzalka, Juraj},
Title = {Ludovit Stur - on the Permanence of Romantic Leadership},
Journal = {SLOVENSKY NARODOPIS-SLOVAK ETHNOLOGY},
Year = {2017},
Volume = {65},
Number = {1},
Pages = {65-76},
Abstract = {The essay aims to describe some popular patterns of romantic leadership
   in Slovakia through the example of the continuing popularity of the
   image of Ludovit Stur, leader of the Slovak national movement. The main
   argument is that the image of a romantic leader fighting against
   ``foreign{''} plots is not only the product of the social organisation
   of post-socialist Slovak politics or the effect of bucolic Slovak
   nationalism, but that the image of the leader as a social actor, his
   self-presentation and story, reproduced across political-economic
   regimes, influence the permanent popularity of romantic leadership in
   Slovakia.},
