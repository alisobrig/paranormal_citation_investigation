@article{ WOS:000482786900001,
Author = {Moodley, Prevan and Lesage, Schvaughn Sandrine},
Title = {A discourse analysis of Ebola in South African newspapers (2014-2015)},
Journal = {SOUTH AFRICAN JOURNAL OF PSYCHOLOGY},
Year = {2020},
Volume = {50},
Number = {2},
Pages = {158-169},
Month = {JUN},
Abstract = {The 2014 Ebola outbreak in three African states transformed the virus
   into a social reality in which media representations contributed to
   globalised hysteria and had rhetorical effects. This study investigated
   representations of the Ebola virus/disease in South African news reports
   (March 2014-June 2015). Four discourses were found to operate within the
   globalised social context: threat to humanity, predation, invasion, and
   conspiracy. The South African reportage framed Ebola as a predator and
   criminal rather than using stock warfare imagery. Representations
   indicated alignment with phobic high-income countries and colonial
   hegemony.},
