@article{ WOS:000211326900003,
Author = {Kouzmin, Alexander and Johnston, Judith and Thorne, Kym},
Title = {Economic SCADs The Dark Underbelly of Neoliberalism},
Journal = {PUBLIC INTEGRITY},
Year = {2011},
Volume = {13},
Number = {3},
Pages = {221-237},
Abstract = {Economic state crimes against democracy (E-SCADs)-are aimed primarily at
   financial gain. Representative government, based on New Public
   Management's ``lean and mean{''}assumptions, is increasingly vulnerable
   to E-SCADs because of deregulation and the growing use of outsourcing,
   privatization, no-bid contracts, and public-private partnerships (PPPs).
   Examples are presented of E-SCAD vulnerabilities in Australian PPPs for
   constructing large capital facilities. The Australian experience
   encompasses a geographically defined case, but the issues are not
   country-specific; they have wide relevance. If E-SCADs are being
   committed more universally within the PPP model around the world, a much
   greater and higher-level series of E-SCADs may be at play, especially in
   policy contexts of contracting in the ``hollowed-out{''}state.},
