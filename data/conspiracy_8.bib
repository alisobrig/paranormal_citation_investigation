@article{ WOS:000253410600011,
Author = {Gardener, T. and Moffat, J.},
Title = {Changing behaviours in defence acquisition: a game theory approach},
Journal = {JOURNAL OF THE OPERATIONAL RESEARCH SOCIETY},
Year = {2008},
Volume = {59},
Number = {2},
Pages = {225-230},
Month = {FEB},
Abstract = {Why do so many major defence contracts fail to deliver to the
   contractually agreed performance, time and cost requirements? This paper
   identifies the conspiracy of optimism as an important factor in the
   initiation of many projects. Using a combination of Game Theory and
   participatory workshops, we formulate a theory on the conspiracy of
   optimism and test it experimentally. This work forms part of a culture
   and behaviour change initiative within Defence Acquisition involving the
   Ministry of Defence and many defence contractors.},
