@article{ WOS:000895989400001,
Author = {Stubenvoll, Marlis},
Title = {Investigating the Heterogeneity of Misperceptions: A Latent Profile
   Analysis of COVID-19 Beliefs and Their Consequences for
   Information-Seeking},
Journal = {SCIENCE COMMUNICATION},
Year = {2022},
Volume = {44},
Number = {6},
Pages = {759-786},
Month = {DEC},
Abstract = {This panel study in Austria in 2020 (N-W1 = 912, N-W2 = 511) explores
   distinct audience segments regarding beliefs in misinformation,
   conspiracy, and evidence statements on COVID-19. I find that citizens
   fall into seven segments, three of which endorse unsupported claims: The
   threat skeptics selectively accept misinformation and evidence; the
   approvers tend to accept all types of information; and the misinformed
   believe in misinformation and conspiracy statements while rejecting
   evidence. Further analyses suggest that the misinformed increasingly
   sought out COVID-19 threat-negating information from scientific sources,
   while also overall attending to threat-confirming information. These
   patterns have practical implications for correcting misperceptions.},
