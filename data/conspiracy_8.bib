@article{ WOS:000182783800009,
Author = {Décarie, D},
Title = {The theme of the traitor and the plot:: La `Mise en scene' by
   Claude!Ollier},
Journal = {ETUDES FRANCAISES},
Year = {2003},
Volume = {38},
Number = {3},
Pages = {109-133},
Abstract = {Ba Iken, Lassalle's interpreter, the ``hero{''} of Claude Ollier's La
   Mise en scene, is also a ``traitor{''} and as such the authenticity of
   his translation must be questioned. The interpreter's ``treason{''}
   reveals the fragility of all ``interpretation{''}. The most important,
   for Ollier, is the plot (conspiracy) hidden behind the act of betrayal.
   To unveil the conspiracy - the major mode of treason which is in fact
   the relationship of the narrative to the world -, one must go back to
   Aristotle's concept of mimesis. Only then can we begin to perceive the
   full breadth literature, adding to the idea of ``inspiration{''} from
   reality, that of ``expiration{''} in a reality constantly being
   re-staged and re-played, as in La Mise en scene.},
