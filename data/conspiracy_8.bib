@article{ WOS:000886356200001,
Author = {Modonutti, Ring},
Title = {ALBERTINO MUSSATO, CICERONE E LA CONGIURA DI NICCOLO DA LOZZO (LETTURA
   DI DE GESTIS HENRICI VII CESARIS X RUBB. 2-3)},
Journal = {RIVISTA DI LETTERATURA STORIOGRAFICA ITALIANA},
Year = {2021},
Volume = {5},
Pages = {9-27},
Abstract = {Albertino Mussato, Cicero and the conspiracy of Niccoh3 da Lozzo
   (reading of Dcgestis Henrici VII Cesaris x rubb. 2-3) `` This essay
   discusses the second rubric in Book x of Albertino Mussato's De gestis
   Henrici vii Cesaris, reporting the conspiracy that the Paduan nobleman
   Niccola da Lozzo plotted against the communal government of his own city
   in December 1312. In particular, this study intends to show how
   Mussato's text tries to present the author himself as a new Cicero
   during his consulship at the time of Catiline, that is, the Roman
   revolutionary's main opponent. The account of this event in Mussato's De
   gestis is also tied to Lozzo's portrait in the De generatione aliquorum
   civium urbis Padue tam nobilium quam ignobiliam by Giovanni da Nono,
   another Paduan historian and a contemporary of Mussato.},
