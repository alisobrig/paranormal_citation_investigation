@article{ WOS:000899325500001,
Author = {Ballantyne, Nathan and Celniker, Jared B. B. and Dunning, David},
Title = {``Do Your Own Research{''}},
Journal = {SOCIAL EPISTEMOLOGY},
Year = {2022},
Month = {2022 DEC 19},
Abstract = {This article evaluates an emerging element in popular debate and
   inquiry: DYOR. (Haven't heard of the acronym? Then Do Your Own
   Research.) The slogan is flexible and versatile. It is used frequently
   on social media platforms about topics from medical science to financial
   investing to conspiracy theories. Using conceptual and empirical
