@article{ WOS:000217499300005,
Author = {Garcia Tojar, Luis},
Title = {11-M: a lesson on greedy journalism},
Journal = {INTERNATIONAL REVIEW OF SOCIOLOGY-REVUE INTERNATIONALE DE SOCIOLOGIE},
Year = {2010},
Volume = {20},
Number = {1},
Pages = {77-91},
Abstract = {Through the analysis of Spanish media coverage of the terrorist attack
   in Madrid 11 March 2004, and specifically of the so-called `conspiracy
   theory' sustained by certain media for political or commercial reasons
   with large public success, this article examines the consolidation in
   Spain of a kind of journalism that tries to model with Lewis Coser's
   concept of `greedy institution'. The political functions of this `greedy
   journalism' and its relationship with the so-called `neo-populist turn'
   of present politics in Western democracies are discussed in brief.},
