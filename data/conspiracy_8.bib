@article{ WOS:000359290400006,
Author = {Halloran, Kevin},
Title = {A Murder at Pucklechurch: The Death of King Edmund, 26 May 946},
Journal = {MIDLAND HISTORY},
Year = {2015},
Volume = {40},
Number = {1},
Pages = {120-129},
Month = {SPR},
Abstract = {This article re-examines the circumstances of King Edmund's murder at
   Pucklechurch, Gloucestershire in May 926. It argues that the accepted
   version that the death was a mischance arising from a brawl with an
   outlaw is unlikely and is based on medieval accounts that are clearly
   contradictory and unconvincing. It suggests that the persona of the
   king's killer as the thief, Leofa, was a construct by later chroniclers
   designed to counter rumours that the king was the victim of a political
   assassination.},
