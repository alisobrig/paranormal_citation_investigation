@article{ WOS:000428597600005,
Author = {Matheson, Calum Lister},
Title = {Psychotic Discourse: The Rhetoric of the Sovereign Citizen Movement},
Journal = {RHETORIC SOCIETY QUARTERLY},
Year = {2018},
Volume = {48},
Number = {2},
Pages = {187-206},
Abstract = {Reactionary conservative groups such as the sovereign citizen movement
   are increasingly prominentand violentin the United States. These groups
   cohere around their own unique discourses of law, language, and history,
   which are often dismissed as meaningless, or even crazy. Following
   Jacques Lacan's injunction that the analyst must let the subject speak,
   this essay will examine sovereign citizen rhetoric as a coherent,
   internally consistent field of meaning exhibiting the traits of
   psychotic discourse in which the metaphorical operation of the
   law-as-signifier is disavowed. Doing so illustrates not only the
   powerful intersection of communication and psychoanalysis but also the
   potential for a rhetorical reading to challenge the most violent
   collective psychoses.},
