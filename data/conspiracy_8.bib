@article{ WOS:A1995RU91300004,
Author = {LACASSE, C},
Title = {BID RIGGING AND THE THREAT OF GOVERNMENT PROSECUTION},
Journal = {RAND JOURNAL OF ECONOMICS},
Year = {1995},
Volume = {26},
Number = {3},
Pages = {398-417},
Month = {FAL},
Abstract = {In this article, I develop a simple bidding model in which collusion is
   endogenous. Buyers at a first-price sealed-bid auction decide whether to
   rig their bids given that they face the threat of government
   prosecution. A legal authority chooses whether to investigate the buyers
   on the basis of the bids tendered. In the unique sequential equilibrium
   of the game, buyers rig their bids with positive probability, but the
   legal authority can never ascertain, on the basis of the bids alone,
   that a conspiracy has formed.},
