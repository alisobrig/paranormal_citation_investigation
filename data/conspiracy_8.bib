@article{ WOS:000411501400020,
Author = {Perucci, Tony},
Title = {The Trump is Present},
Journal = {PERFORMANCE RESEARCH},
Year = {2017},
Volume = {22},
Number = {3},
Pages = {127-135},
Abstract = {Among the many unprecedented aspects of the 2016 U.S. Presidential
   election was the emergence of the term performance art' to describe
   Donald Trump's campaign. As unexpected as this rhetoric might seem, when
   read in the context of populist appeals to eliminate the aesthetic gap
   of political representation, we can see that Trump shares performance
   art's claims to access a direct and unmediated real. This commonality is
   underlined by such discourse's references to celebrity performance
   artist', Marina Abramovi. In conspiracy theories and hoaxes during the
   campaign, Abramovi was represented both as the architect of a
   child-sex-slavery ring and as the mastermind behind the secret
   performance art piece that went too far' that was Trump's campaign.
   Alternating between conspiracy theory's suspension of disbelief and a
   fictional hoax's suspension of belief, performance art' articulates the
   crisis of belief in belief that propelled Donald Trump to the White
   House.},
