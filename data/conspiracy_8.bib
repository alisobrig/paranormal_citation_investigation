@article{ WOS:000226053600009,
Author = {Mitjana, JPI},
Title = {Assemblies, gatherings and conspiracy in the crisis of Isabella's
   regime. The case of the city of Barcelona, 1867-1868},
Journal = {HISPANIA-REVISTA ESPANOLA DE HISTORIA},
Year = {2004},
Number = {217},
Pages = {689-710},
Month = {MAY-AUG},
Abstract = {The aim of the present study is the description of the activity and
   organisation of the revolutionaries in Barcelona in the period preceding
   the victory of the Spanish Democratic Revolution of 1868. The activities
   of those opposition groups, and the clandestine revolutionary Committee
   existing from 1867, are studied through the analysis of A. Feliu
   Codina's memoirs. The outbreak of the revolution in Barcelona, where the
   committees were mainly neither republicans nor federalists, enhanced the
   diffusion of the federal republicanism and transformed an initial
   pronunciamiento into a true democratic revolution, Barcelona becoming
   the pioneer of the Spanish federal republicanism.},
