@incollection{ WOS:000295618900002,
Author = {Fantham, Elaine},
Editor = {Asso, P},
Title = {A CONTROVERSIAL LIFE},
Booktitle = {BRILL'S COMPANION TO LUCAN},
Year = {2011},
Pages = {3-20},
Abstract = {We know very little about Lucan's personal life but his artistic dispute
   with Nero has been a matter of controversy in ancient and modern
   scholarship. Forbidden to perform his poetry, Lucan discovered the need
   for a coup and joined a conspiracy remarkable for its foolish intention
   (to replace one admittedly demented ruler with another more conventional
