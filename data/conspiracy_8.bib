@article{ WOS:000215683200010,
Author = {Polo Blanco, Jorge},
Title = {Capitalism, fascism and democracy in the work of Karl Polanyi. A
   crossroads still alive},
Journal = {ENCRUCIJADAS-REVISTA CRITICA DE CIENCIAS SOCIALES},
Year = {2014},
Volume = {7},
Pages = {133-152},
Abstract = {This essay reconstructs some of the vectors implicit to the historical
   development of the European modernity following the analysis of Karl
   Polanyi. This political and historical crossroads is related to a
   long-term conflict between free-market economy and democracy. It is a
   complex theoretical and practical field open to different itineraries
   that include the tragedy of fascism. The interpretation of Karl Polanyi
   of this historical dynamic is conceptually powerful and more up-todate
   than ever.},
