@article{ WOS:000211734100002,
Author = {Welch, Dennis},
Title = {Blake, the Famine of 1795, and the Economics of Vision},
Journal = {EUROPEAN ROMANTIC REVIEW},
Year = {2007},
Volume = {18},
Number = {5},
Pages = {597-622},
Abstract = {This study shows that a significant historical context underlying
   Blake's prophecies of 1795 was the famine in that year and the era's
   pervasive discourse on market economics, whose firm support of agrarian
   interests provoked serious doubt and much opposition to relying on
   naturalistic reactions to the crisis and neglecting more imaginative and
   humane responses. As Blake's work and the history of 1794-95 show, there
   permeated a sense of conspiracy about economic, political, and even
   religious interests colluding in the manipulation of scarcity and
   abundance. Blake's response to the crisis involved reformulating the
   divisive economics of profit and loss in spiritual terms, especially
   those of Eternity and its inclusive interplay of desire and fulfillment.
   His revision of the economics of finitude sought to expose-appropriately
   through the mythic figure Los(s)-its invisible forces, mystifications,
   and deceptions as represented by Urizen, Fuzon, the Tree of Mystery, and
   the ``Human Illusion.{''} Witnessing his society's hard times, Blake
   advocated a different dispensation-a more valuable economy-in which
   loss, sacrifice, and brotherhood yield the greatest spiritual and
   personal gains.},
