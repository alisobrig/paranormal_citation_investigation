@article{ WOS:000173952500008,
Author = {Irurozqui, M},
Title = {``The war of civilization{''}, The indigenous participation in the
   revolution of 1870 in Bolivia},
Journal = {REVISTA DE INDIAS},
Year = {2001},
Volume = {61},
Number = {222},
Pages = {407-432},
Month = {MAY-AUG},
Abstract = {Through the analysis of the literary and political production of Casimro
   Corral and the documents stemming from his administrative and military
   conspiracies, this article gives a new interpretation of the 1870
   revolution as the moment when the indigenous population of the La Paz
   Altiplano acquired public inclusion and national conscience. With the
   aim of analyzing the significance of the State institutions in the
   redefinition of previous collective identities, as well as their
   transformation from corporative to national identities, this article
   discusses the nature, reach and reception of the civic regeneration
   project proposed by the revolutionaries. Two phases are distinguished,
   that of the conflict itself and that of the following months. In this
   framework it is examined the meaning given by revolutionaries to the
