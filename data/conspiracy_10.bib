@article{ WOS:000570670900014,
Author = {Glowacki, Elizabeth M. and Taylor, Mary Anne},
Title = {Health Hyperbolism: A Study in Health Crisis Rhetoric},
Journal = {QUALITATIVE HEALTH RESEARCH},
Year = {2020},
Volume = {30},
Number = {12, SI},
Pages = {1953-1964},
Month = {OCT},
Abstract = {The Ebola virus had only been in the United States for 2 months before
   it became a major national health concern. However, while some citizens
   panicked about the looming health crisis, others remained calm, offering
   explanations for why a rapid spread of the virus was unlikely. Examining
   the distinctions between these different reactions can contribute to a
   better understanding of the coping strategies citizens use when facing a
   health crisis. We consider how citizens respond to fear by focusing on
   whether or not hyperbolic rhetoric was used as a means for processing
   and managing fear. Approximately 400 tweets and Facebook posts from the
   Centers for Disease Control and Prevention, the White House, andThe Alex
   Jones Showwere examined to make conclusions about how citizens respond
   to messages from these mediated forums. At the intersection of health
   communication and critical rhetoric, we advance an operational
   definition of health hyperbolism derived from public response to opinion
   leaders. Ultimately, we find that health hyperbolism contains language
   illustrative of distrust, blame, anger, misrepresentation, conspiracy,
   and curiosity.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
