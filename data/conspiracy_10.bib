@article{ WOS:000936908800022,
Author = {McAlister, Melani},
Title = {Narrative Disorders: Surveillance, Truth, and Democracy in Malka Older's
   Centenal Cycle},
Journal = {AMERICAN LITERARY HISTORY},
Year = {2023},
Volume = {35},
Number = {1, SI},
Pages = {305-319},
Month = {FEB 20},
Abstract = {Malka Older's Centenal Cycle comprises three novels that, taken
   together, reimagine democracy after the end of the nation-state. The
   series tells an eerily timely story of a late twenty-first-century
   global election being undermined by an attack on the election apparatus,
   run by a ubiquitous surveillance system called Information. When
   Information is attacked, the global election goes haywire, and our
   protagonists, most of whom work for Information, are forced to dash
   around the world trying to save the system. Whether the system is worth
   saving and what democracy needs to survive are two of the questions that
   occupy the multi-racial and cosmopolitan women at the center of this
   sprawling science fiction world, richly conceptualized by Older, a
   Latinx writer and ``Senior Global Futures Scientist `` at Arizona State
   University.The novels postulate that we cannot radicalize and
   reinvigorate democracy without reducing its scale. The price for that
   will be expansions of technological reach and the real peril of global
   conspiracy across small states. In that world, narrative is currency,
   and overreading a practice of freedom.},
