@article{ WOS:000480061100001,
Author = {Rozano-Garcia, Francisco J.},
Title = {``Unfer maoelode{''}: The Villain in <i>Beowulf</i> Reconsidered},
Journal = {ENGLISH STUDIES},
Year = {2019},
Volume = {100},
Number = {8},
Pages = {941-958},
Month = {NOV 17},
Abstract = {The many obscurities surrounding the origin of Unfer in the Old English
   epic Beowulf, his background, his intentions, or even the meaning of his
   very name, have fascinated readers and scholars alike throughout the
   past two centuries. However, despite all this attention, we still know
   next to nothing about the person and motivations of Unfer. The reasons
   for this are mainly two: scarce and obscure evidence, and lack of
   consensus as to how it should be interpreted. This paper undertakes
   revision of the evidence available and of previous scholarship, through
   use of linguistic and textual analysis of the misunderstood etymology of
   Unfer's name, his rank, and the role he plays in Beowulf. By the end of
   the present discussion, the shadow of conspiracy that has always
   accompanied Unfer will be proved untenable, and the courtier restored to
   a more accurate position as spokesman of the Danish king Hrogar.},
