@inproceedings{ WOS:000074439200025,
Author = {Ghodosi, H and Pieprzyk, J and Charnes, C and Safavi-Naini, R},
Editor = {Pieprzyk, J and Seberry, J},
Title = {Cryptosystems for hierarchical groups},
Booktitle = {INFORMATION SECURITY AND PRIVACY: 1ST AUSTRALASIAN CONFERENCE, ACISP 96},
Series = {Lecture Notes in Computer Science},
Year = {1996},
Volume = {1172},
Pages = {275-286},
Note = {1st Australasian Conference on Information Security and Privacy,
   WOLLONGONG, AUSTRALIA, JUN 24-26, 1996},
Organization = {Australasian Soc Electr Security; Univ Wollongong, Ctr Comp Security
   Res; Int Assoc Cryptol Res},
Abstract = {This paper addresses the problem of information protection in
   hierarchical groups. Higher level groups of participants can control the
   information flow (the decryption ability) to lower level groups. If a
   higher level group decides to allow a lower level group to read the
   message, it passes a go ahead ticket so the lower level group can
   decrypt the cryptogram and read the message. The formal model of
   top-down hierarchical cryptosystems is given.
   Two practical and efficient schemes are described. The first is based on
   the ElGamal system. The second applies the RSA system. In proposed
   schemes the dealer publishes a public key such that an individual can
   use it to send an encrypted message to the (hierarchical) group.
   Publication of both the group public key and the encryption method does
   not reveal the decision of the group. The proposed cryptosystems are
   immune against conspiracy attack.
   The lack of verifiability of retrieved messages in threshold ElGamal
   cryptosystems is also discussed.},
