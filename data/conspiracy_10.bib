@article{ WOS:000246440200002,
Author = {Fletcher, George P.},
Title = {<i>Hamdan</i> confronts the Military Commissions Act of 2006},
Journal = {COLUMBIA JOURNAL OF TRANSNATIONAL LAW},
Year = {2007},
Volume = {45},
Number = {2},
Pages = {427-467},
Abstract = {In 2006 the law of war experienced two major shock waves. The first was
   the decision of the Supreme Court in Harridan, which represented the
   first major defeat of the President's plan, based on an executive order
   of November 2001, to use military tribunals against suspected
   international terrorists. The majority of the Court held the procedures
   used in the military tribunal against Hamdan violated common article
   three of the Geneva Conventions. A Plurality of four, with the opinion
   written by Justice Stevens, based their decision as well on
   afar-reaching interpretation of the substantive law of war. They held
   that conspiracy to commit terrorist acts did not fall under the
   customary international law of war. Congress responded by enacting the
   Military Commissions Act of 2006. The President signed the bill on
   October 17. The interpretation of Harridan and the precedents on which
   it is based will shape future litigation about the constitutionality of
   the various provisions of this legislation.},
