@article{ WOS:000967944900003,
Author = {Leporesi, Eleonora},
Title = {The paintings in the Hall of Judgement in the Black Palace of Coredo},
Journal = {RIVISTA D ARTE},
Year = {2021},
Volume = {11},
Pages = {33-54},
Abstract = {The courthouse of Coredo (TN) was built during the 2nd half of the 15th
   century at the behest of the prince-bishop of Trento. It is also called
   the Black Palace. The court room is decorated with an extremely original
   pictorial cycle based on the poem The Queen of France written by the
   German poet Schondoch. It portrays a woman wrongly accused of adultery
   by the court marshal, who is hence forced to leave the castle and to
   live hidden in the forest until the conspiracy is revealed. This
   literary topos is extremely widespread in Medieval popular literature.
   The Black Palace paintings are an absolutely original testimony of
   courtly painting adapted to a court room, which has no comparison in the
   entire Trentino-Alto Adige region. Furthermore, it is the only known
   representation of this poem in painting, characterized by a unique and
   suggestive style. However, the palace is currently a private house.
   There is a pressing need not only to restore, but also to promote, these
   outstanding paintings.},
