@article{ WOS:000313590500002,
Author = {Hooper, Cynthia},
Title = {Trust in Terror? The Search for a Foolproof Science of Soviet Personnel},
Journal = {SLAVONIC AND EAST EUROPEAN REVIEW},
Year = {2013},
Volume = {91},
Number = {1},
Pages = {26+},
Month = {JAN},
Abstract = {Trust in Terror? The Search for a Foolproof Science of Soviet Personnel
   by Cynthia Hooper This article focuses on a job search for Kremlin
   librarians that took place during the Great Terror, in the wake of a
   purge of more than 80 per cent of Kremlin service staff following the
   sensational `discovery' of a purported conspiracy among them to murder
   Josef Stalin. The article argues that the Terror not only prompted
   hysterical hunts for `hidden enemies' within the Soviet bureaucracy, but
   that it also accelerated almost equally frantic efforts on the part of
   Soviet leaders to rebuild trust inside the regime by developing a
   `science' of personnel that would allow observers to infallibly
   distinguish truth from seeming and thus differentiate genuinely
   dedicated servitors from those who were merely pretending to be so.
   Finally, the article dissects the mix of fact and fantasy involved in
   personnel assessment during this time and compares Soviet strategies for
   evaluating personality to those under development in the USA.},
