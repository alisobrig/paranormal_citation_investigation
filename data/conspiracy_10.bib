@article{ WOS:000448584400013,
Author = {Gabriel Soares, Marta Alice},
Title = {SPECTACLES OF POWER: THE SOMATIC TESTIMONY OF ETHEL ROSENBERG AS A
   REENACTMENT OF THE MARTYRDOM OF JOAN OF ARC},
Journal = {OP CIT-REVISTA DE ESTUDOS ANGLO-AMERICANOS-A JOURNAL OF ANGLO-AMERICAN
   STUDIES},
Year = {2012},
Number = {1},
Pages = {173-189},
Abstract = {Electrocuted in 1953 on charges of conspiracy to commit espionage, Ethel
   and Julius Rosenberg stand as a painful reminder of the institutional
   violence that a country's government is capable of exerting on its own
   people to ensure consensus. This paper will focus more specifically on
   Ethel Rosenberg, comparing her execution to the public burning of Joan
   of Arc so as to disclose the purposes underlying her sentence.
   Perceiving Ethel's trial as a spectacle of power whose excesses and
   derealization parallel Joan's process will enable an analysis of
   martyrdom as a somatic testimony, exploring how bodies testify to the
   upheavals of their times. In this paper, I will be particularly
   interested in Ethel's stand vis-a-vis the American body politic of the
   1950s, seeking to demonstrate that her execution, besides punishing high
   treason, served as well to reinforce an ideological framework and to
   consolidate a political narrative shaped by Cold War hysteria.},
