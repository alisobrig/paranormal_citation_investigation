@article{ WOS:000234214700003,
Author = {Verleden, F},
Title = {Hendrik de Man, `a minister in his shirt-sleeves' - Misisterial office
   and political style as interpretation of a new socialism (1935-1938)},
Journal = {TIJDSCHRIFT VOOR GESCHIEDENIS},
Year = {2005},
Volume = {118},
Number = {4},
Pages = {533-550},
Abstract = {The career of the famous Belgian socialist theorist and politician
   Hendrik (Henri) de Man (1885-1953) receives much attention from
   historians and social scientists. Most studies concentrate on his
   importance as a socialist revisionist during the interwar period or his
   subsequent collaboration with nazi-Germany. This article however deals
   with his political career (1933-1940) as vice-president of the
   BelgianWorkers Party {[}BWP] and minister of Public Works and Finance
   (1935-1938). Crucial to the understanding of his policies and behaviour
   as a public figure - which contemporary socialists labelled `non
   conformist', to say the least - is reading of his theoretical works. His
   political style, which didn't fit in the traditional political culture
   of the BWP, aimed at creating a new kind of socialist leader. As
   minister, he tried to implement his vision of a new, post-Marxist
   socialism against increasing resistance. Distrustful by nature, he saw
   conspiracies whenever he suffered a setback, which led to a deep rancour
   against the parliamentary system.},
