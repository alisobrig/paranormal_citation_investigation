@article{ WOS:000697209400019,
Author = {Netto dos Santos, Paulo Eduardo and da Silva Riehl, Carlos Alberto},
Title = {Application of a Didactic Sequence for High School in the Thematic Drugs
   through of the Maguerez Arch for the Demystification of Science},
Journal = {REVISTA VIRTUAL DE QUIMICA},
Year = {2021},
Volume = {13},
Number = {3},
Pages = {836-844},
Month = {MAY-JUN},
Abstract = {The proliferation of information of a fallacious nature has led to the
   dissemination of numerous conspiracy theories and their respective
   groups, such as flat Earth model and the anti-vaxxers. Many of these
   assumptions question some of the basic scientific principles. As a
   result, in recent years, a growing portion of society is perceived that
   observes scientific knowledge with suspicion. The objective of this work
   is to apply a didactic sequence to students graduating from high school
   to discuss the relevance of science and its professional. The strategy
   for questioning was the Maguerez Arc, as it is a methodology that allows
   the protagonist of the student throughout all its stages. The theme
   chosen was drugs, due to its historical and cultural importance for the
   development of civilizations and the numerous impacts on contemporary
   societies. The didactic sequence included activities such as a
   questionnaire, experiments and a project planned and completed by the
   students. The development of the work proved to be a valid strategy for
   approaching the theme, as it provided the protagonism of the students
   and proved reflections on their social representations.},
