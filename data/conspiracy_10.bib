@article{ WOS:000614141500006,
Author = {Roca Vernet, Jordi},
Title = {The political violence of radical liberalism during the Liberal
   Triennium. The defense of the constitutional regime from Barcelona},
Journal = {PASADO Y MEMORIA-REVISTA DE HISTORIA CONTEMPORANEA},
Year = {2020},
Volume = {22},
Pages = {155-186},
Abstract = {This paper deals with the analysis of political violence among Spanish
   liberals during the Liberal Triennium. Different analytical categories
   thereof are established. A second aim is to unveil the authorities'
   attitude towards violence, and to what extent it was linked to the
   spread of fear of the internal enemy within liberal circles. The study
   validates the hypothesis that political violence gradually increased as
   the liberal authorities felt threatened by both internal and external
   enemies. To this added the pressure of exalted liberalism, in the
   context of their demands for popular, or revolutionary justice against
   the enemies of the liberal regime. Coincidentally, the conflict against
   the royalists broke the balance of the institutional powers, which
   significantly led to an increasing level of violence, until military
   power was imposed with the support of both radical and popular
   liberalism.},
