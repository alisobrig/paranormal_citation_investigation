@article{ WOS:000345894300002,
Author = {Blackman, Robert},
Title = {DID CICERO SWEAR THE TENNIS COURT OATH?},
Journal = {FRENCH HISTORY},
Year = {2014},
Volume = {28},
Number = {4},
Pages = {471-497},
Month = {DEC},
Abstract = {Historians of discourse put enormous explanatory weight on speeches
   deputies gave during the French Revolution. It can be difficult, though,
   to be certain that what the deputies said is what the audiences heard.
   here 1 examine a speech made by Honore-Gabriel Riquetti, count of
   Mirabeau on 19 April 1790 in which he compared deputies who had sworn
   the Tennis Court Oath in June of 1789 to Marcus Tullius Cicero, consul
   of the Roman republic who had thwarted a major conspiracy in 63 BCE.
   Mirabeau's brief reference to antiquity provided the turning moment in a
   debate on an unrelated topic. Exploring the impact a reference to
   antiquity had on an important debate will help us to understand the role
   references to classical antiquity could have in the debates of the
   winter and spring of 1790 and allow us to evaluate claims that there was
   something inherently destabilizing about classical republican rhetoric.},
