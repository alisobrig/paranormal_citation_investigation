@article{ WOS:000085327300003,
Author = {Pastourmatzi, D},
Title = {Violence and black childhood},
Journal = {AAA-ARBEITEN AUS ANGLISTIK UND AMERIKANISTIK},
Year = {1999},
Volume = {24},
Number = {2},
Pages = {199-215},
Abstract = {From the slave narratives to the contemporary novels, many black authors
   have delineated heart-wrenching accounts of childhood, connecting this
   formative period with misfortune and victimization. A close examination
   of works by Frederick Douglass, Richard Wright, Albert French, Harriet
   Jacobs, Maya Angelou and Alice Walker leads to the conclusion that black
   childhood and violence go hand in hand. Although childhood has been
   diachronically a more painful experience for a black girl than for a
   black boy, these authors agree that a black child's early acquaintance
   with the injustices of the adult world leads to a premature loss of
   innocence, mutilating the child emotionally, psychologically and
   morally. Furthermore, a muzzle of silence has been placed on black
   children irrespective of gender, trapping them in a conspiracy of
   secrecy and forcing them to suppress abhorrent acts. Collectively, the
   childhood experiences recorded by the aforementioned writers disclose
   the sad fact that even when premature death is evaded, the emotional and
   psychic scarification can never be eradicated from memory.},
