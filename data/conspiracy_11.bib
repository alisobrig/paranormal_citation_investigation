@article{ WOS:000408343600012,
Author = {Urakova, Alexandra P.},
Title = {BETWEEN EUROPE, UNITED STATES AND RUSSIA: IVAN TURGENEV IN THE RECEPTIVE
   AESTHETICS OF HENRY JAMES},
Journal = {VESTNIK SLAVIANSKIKH KULTUR-BULLETIN OF SLAVIC CULTURES-SCIENTIFIC AND
   INFORMATIONAL JOURNAL},
Year = {2017},
Volume = {43},
Pages = {152-160},
Month = {MAR},
Abstract = {The Russian author Ivan S. Turgenev became a ``pass to Europe{''} of a
   kind to the American, Henry James, in that he introduced the latter to
   the first-rate Parisian literary circles. James read Turgenev in German
   and French (despite existing English translations) and included him as
   the only foreigner in his book French Poets and Novelists (1878)
   together with Baudelaire, Balzac, Sand, Flaubert, Merimee, etc. At the
   same time, the question of Turgenev's national identity is a recurring
   topic of Jamesian essays. In these essays, Turgenev is at once European
   and non-European: an acknowledged European author yet belonging to the
   culture that represents European otherness, as it were. Eventually, he
   is a non-existing American classic: a novelist who, according to James,
   would have been a great American writer if born in the American South.
   This essay scrutinizes the question of Turgenev's national identity and
   his ``Europeanness{''} on the material of Jamesian essays belonging to
   different genres: reviews, a mortuary, and an encyclopedic article. It
   also shows how James attempts to ``Europeanize{''} Turgenev by remaking
   his novel Virgin Soil. His own novel, Princess Casamassima considered
   imitative by many critics, not only transfers the conspiracy plot to
   Europe but rewrites it according to the rules that James associated with
   Western European literary tradition.},
