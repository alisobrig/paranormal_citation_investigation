@article{ WOS:000313864100003,
Author = {McDonald, Matthew},
Title = {ANTITRUST IMMUNITY UP IN SMOKE: PREEMPTION, STATE ACTION, AND THE MASTER
   SETTLEMENT AGREEMENT},
Journal = {COLUMBIA LAW REVIEW},
