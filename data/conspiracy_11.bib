@article{ WOS:000704308700002,
Author = {Tambuscio, Marcella and Oliveira, Diego F. M. and Ciampaglia, Giovanni
   Luca and Ruffo, Giancarlo},
Title = {Network segregation in a model of misinformation and fact-checking},
Journal = {JOURNAL OF COMPUTATIONAL SOCIAL SCIENCE},
