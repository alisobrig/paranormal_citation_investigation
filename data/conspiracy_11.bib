@article{ WOS:000780980500013,
Author = {Sorensen, Roy},
Title = {Lie for me: the intent to deceive fails to scale up},
Journal = {SYNTHESE},
Year = {2022},
Volume = {200},
Number = {2},
Month = {APR},
Abstract = {To understand lying, we naturally focus on small scale lies involving
   one speaker, one listener, one assertion. This methodology confers
   artificial plausibility upon the requirement that liars intend to
   deceive. For it excludes principal-agent conflicts that emerge from
   linguistic division of labor. When an employee lies for her boss, she
   need not inherit his motive to deceive. She displays loyalty even if her
   lie does not deceive. Focus on a single lie in isolation also blinds us
   to tactical deceptions such as telling a lie which is intended to be
   caught to advance another lie. Many of these complexities arise from
   situations that approximate common knowledge without quite crystallizing
   into this uniform transparency.},
