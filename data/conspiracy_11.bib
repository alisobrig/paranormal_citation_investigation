@article{ WOS:001022831900001,
Author = {Bennett, Andrew},
Title = {Polarization \& populist backlash in transitioning Tunisia: the role of
   internet media \& media ownership},
Journal = {BRITISH JOURNAL OF MIDDLE EASTERN STUDIES},
Year = {2023},
Month = {2023 JUL 5},
Abstract = {Since it's revolution, Tunisia's road to democracy has been
   turbulent-with apathy, polarization, and politically motivated violence
   stifling its transition. While much has been made of internet media's
   role during Tunisia's revolution, far fewer studies have examined its
   continuing effects throughout the country's ongoing democratic
   transition. This study draws on original interviews conducted with
   politicians, journalists, and democracy activists immediately following
   Tunisia's 2019 presidential elections to highlight how internet media
   and media ownership have hindered democratic consolidation. Since its
   revolution, internet media has stood as a central pillar of Tunisia's
   political discourse-but despite the country's many subsequent gains in
   informational freedom and media pluralism, I find that concentrated
   ownership and weak regulation over the internet/media have created space
   for powerful elites, former regime officials, and other anti-democratic
   forces to marshal influence over the online marketplace to stifle and
   push back against democratic consolidation. Overall, concentrated media
   markets-especially online-have empowered elite owners to impede media
   regulation and disseminate an unchecked deluge of bias, misinformation,
   conspiracies, and extremism-thereby propelling the kinds of deepening
   divisions, polarization, and mistrust towards the democratic process
   that has given rise to more populist/authoritarian actors seeking to
   erode democratic institutions and seize political power.},
