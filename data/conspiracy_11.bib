@article{ WOS:000859443200001,
Author = {Ellett, Lyn and Schlier, Bjoern and Kingston, Jessica L. and Zhu, Chen
   and So, Suzanne Ho-Wai and Lincoln, Tania M. and Morris, Eric M. J. and
   Gaudiano, Brandon A.},
Title = {Pandemic paranoia in the general population: international prevalence
   and sociodemographic profile},
Journal = {PSYCHOLOGICAL MEDICINE},
