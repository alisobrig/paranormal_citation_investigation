@article{ WOS:001022543000022,
Author = {Starikova, Nina V. and Shurshikova, Anna V. and Nochvina, Bella A.},
Title = {``Magical{''} Practices in Daily Life of Nizhny Novgorod Peasants in
   Second Half of 18th - First Half of 19th Centuries},
