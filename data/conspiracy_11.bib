@article{ WOS:000893193900004,
Author = {Nubowo, Andar},
Title = {Covid-19, Fatwas, and Socio-religious Praxis Muhammadiyah's Social
   Engagement and Mission in Coping with the Outbreak in Indonesia},
Journal = {SOCIAL SCIENCES AND MISSIONS-SCIENCES SOCIALES ET MISSIONS},
Year = {2022},
