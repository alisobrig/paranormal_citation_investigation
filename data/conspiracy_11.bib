@article{ WOS:000753125700003,
Author = {Best, Janice},
Title = {PLAYING NAPOLEON AT THE DAWN AND DURING THE SECOND EMPIRE: A NEW MYTH OF
   THE `LITTLE CAPORAL'},
Journal = {FRENCH STUDIES},
Year = {2020},
Volume = {74},
Number = {3},
Pages = {383+},
Month = {JUL 1},
Abstract = {This article offers an analysis of three plays about Napoleon Bonaparte
   performed at the Cirque national theatre at the end of the Second
   Republic and in the early years of France's Second Empire: Bonaparte en
   Egypte by Fabrice Labrousse and Narcisse Fournier, Le Consulat et
   l'Empire by Labrousse and Albert, and La Barrie're de Clichy by Paul
   Meurice, Alexandre Dumas pe're, and Auguste Maquet. Intended for a
   mainstream audience, these mimodrames allowed Louis-Napole ` on
   Bonaparte's government to manipulate historical narratives about his
   famous uncle in order to strengthen his own power. Although based on
   historical facts, these plays about Napoleon were nonetheless subject to
   pre-performance censorship, as were all dramatic works at that time. My
   analysis highlights how the changes required by the censors altered the
   historical narrative in these plays, erasing Bonaparte's revolutionary
   origins, and minimizing any suggestion of a conspiracy against him or
   opposition to his coup d'etat. These plays created a founding myth of
   the French nation, based on a story of progress and continuity, that
   effectively silenced any suggestion of revolution or regime change.},
