@article{ WOS:000721843000001,
Author = {Hamdan, M. Bou and Singh, S. and Polavarapu, M. and Jordan, T. R. and
   Melhem, N. M.},
Title = {COVID-19 vaccine hesitancy among university students in Lebanon},
