@article{ WOS:000372797200008,
Author = {Philipps, Ina-Maria and Schmauch, Ulrike and Sielert, Uwe and Valtl,
   Karlheinz and Walter, Joachim},
Title = {Campaigns Against Emanzipatory Sex Education. Statement by the Academic
   Council of the Dortmund Institute for Sex Education (isp)},
Journal = {ZEITSCHRIFT FUR SEXUALFORSCHUNG},
Year = {2016},
Volume = {29},
Number = {1},
Pages = {73+},
Month = {MAR},
Abstract = {This contribution considers the vehement criticism of emancipatory sex
   education in childhood and adolescence in fundamental catholic and
   evangelical circles and in particular the traditionalist sections of the
   popular rightwing party AfD as well as the corresponding online news
   portals in Germany and other German-speaking countries. In the
   statement, the central key words of the opponents -{''} opinions
   cartel{''}, ``early sexualization{''}, ``gender madness{''} and
   ``indoctrination{''} are examined. It is found that ideologically-guided
   imputations, defamations and conspiracy-theoretical assumptions are
   behind these terms. This comes about according to the observations of
   the authors with the sexual-political intention of creating uncertainty
   and fear in the population and in particular amongst parents by means of
   populist disinformation. As a result, the carefully achieved progress
   regarding the recognition of the equality of all people, their dignity
   and right to decide about their own sexuality, is called into question
   and endangered. The statement makes it clear that both the
   fundamental-religious and politically right-wing groups and individuals
   do not themselves actually have either a theoretically-founded concept
   of sexuality or any scientifically-founded sex education concepts.},
