@article{ WOS:000520842700064,
Author = {Davletbaeva, Diana Nyailevna and Demirbash, Guzaliya Sayfullovna
   Khazieva and Ivanova, Alyona Mikhailovna and Ibragimov, Eichin},
Title = {The Theonym Khizyr-Ilyas in Local Traditions of the Tatars and Its
