@article{ WOS:000812562100004,
Author = {Rothaus, Barry},
Title = {Armand-Marc comte de Montmorin, the French monarchy and the Revolution
   in 1792},
Journal = {FRENCH HISTORY},
Year = {2022},
Volume = {36},
Number = {1},
Pages = {44-67},
Month = {JUN 18},
Abstract = {Armand-Marc comte de Montmorin (1746-1792) was the last foreign minister
   (1787-1789) of the old regime and the first of the constitutional
   monarchy (1789-1791). Descended from a prominent Auvergnat aristocratic
   family, he held a very close relationship with the dauphin who became
   king in 1774. Montmorin's diplomatic career began with royal support,
   first as minister to Treves (1775-1778), then as ambassador to Madrid
   (1778-1784). In 1787, Louis XVI named him to head the foreign ministry
   as successor to the comte de Vergennes. The outbreak of revolution led
   to his involvement in domestic affairs. He dedicated his life after 1789
   to saving the Crown, urging the royal couple to adopt the newly emerging
   constitution. Montmorin resigned his office in October 1791; thereafter,
   he was engaged in various conspiracies. In August 1792, following the
   overthrow of the monarchy, the Legislative Assembly arrested, tried and
   convicted him of treason. Held in l'Abbaye prison, he was one of those
   massacred on September 2. His late career provides a unique window into
