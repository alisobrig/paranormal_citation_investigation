   Sanna, Leonardo, Univ Modena \& Reggio Emilia, Dept Studies Language \& Culture, Modena, Italy.},
DOI = {10.1080/02691728.2021.1954716},
EarlyAccessDate = {AUG 2021},
ISSN = {0269-1728},
EISSN = {1464-5297},
Keywords = {Anti-vaccinism; media analysis; post-truth; semiotics},
