@article{ WOS:000174929200004,
Author = {La Rooij, MF},
Title = {From colonial conservative to international antisemite: The life and
   work of Authur Nelson Field},
Journal = {JOURNAL OF CONTEMPORARY HISTORY},
Year = {2002},
Volume = {37},
Number = {2},
Pages = {223-239},
Month = {APR},
Abstract = {Antisemitism and the myth of the Jewish World Conspiracy were important
   and pervasive ideologies of the turbulent interwar period. Across the
   globe, antisemitism was a powerful expression of the growing insecurity
   felt by millions in a world racked by crisis and instability. Among
   those attracted to antisemitism was New Zealander Arthur Nelson Field.
   A.N. Field developed his radical anti-Jewish views in response to the
   shock of the Great Depression. From 1931, when he published his
   best-selling book The truth about the Slump, Field developed a solid
   reputation in New Zealand and internationally as a leading antisemitic
   agitator. In his writings Field sought to provide explanation for the
   causes of the first world war, the decline of the British Empire and the
   origins of the Great Depression. Although his influence has been
   generally overlooked, the study of Field's life reveals much about New
   Zealand's political culture during the 1930s and the character of
   antisemitism as an international ideology. This article offers an
   examination of Field's life and work, his development as an antisemite
   and an assessment of his influence in New Zealand and internationally.
   The article argues that while Field was an extraordinary individual, his
   political development as an antisemite was not completely out of
   character with the period and reflected many of the fears and anxieties
   felt by New Zealanders during the Great Depression.},
