@article{ WOS:000711285100001,
Author = {Boudry, Maarten},
Title = {Diagnosing Pseudoscience - by Getting Rid of the Demarcation Problem},
Journal = {JOURNAL FOR GENERAL PHILOSOPHY OF SCIENCE},
