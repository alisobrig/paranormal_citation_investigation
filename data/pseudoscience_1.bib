@article{ WOS:000216041500001,
Author = {Martin Gaitan, Leandro},
Title = {CAN NEUROTHEOLOGY BE CONSIDERED A SCIENTIFIC DISCIPLINE?},
Journal = {STOA},
Year = {2012},
Volume = {3},
Number = {6},
Pages = {5+},
Month = {JUL-DEC},
Abstract = {This article attempts to define the scientific status of
   ``neurotheology{''}. The most important reasons for which it raises this
