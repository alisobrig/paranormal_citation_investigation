@article{ WOS:000437103400009,
Author = {Mercier, H. and Majima, Y. and Miton, H.},
Title = {Willingness to transmit and the spread of pseudoscientific beliefs},
Journal = {APPLIED COGNITIVE PSYCHOLOGY},
Year = {2018},
Volume = {32},
Number = {4},
Pages = {499-505},
Month = {JUL-AUG},
Abstract = {Pseudoscientific beliefs are widespread and can be damaging. If several
   studies have examined the factors leading people to accept
   pseudoscientific beliefs, no attention has been paid to the factors
   contributing to people's willingness to transmit these beliefs. To test
   whether the willingness to transmit pseudoscientific beliefs contributes
   to their spread, independent of their believability, we asked
   participants to rate statements corresponding either to pseudoscientific
   beliefs (Myths), or to their (correct) negations (Non-Myths). Statements
   were rated on believability, on how willing participants would be to
   transmit them, and on how knowledgeable they would make someone who
   produces them. Results revealed that participants who believed in Myths
   were more willing to transmit them than the participants who believed in
   Non-Myths were willing to transmit Non-Myths. A potential factor driving
   the increased willingness to transmit both Myths and Non-Myths might be
   participants' belief that holding the beliefs makes one seem more
   knowledgeable.},
Publisher = {WILEY},
Address = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
