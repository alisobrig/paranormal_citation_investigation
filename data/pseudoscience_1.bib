@article{ WOS:000430380600019,
Author = {Uskola Ibarluzea, Araitz},
Title = {Skepticism of Elementary preservice teachers about pseudosciences:
   influence of erroneous conceptions in the case of homeopathy},
Journal = {PROFESORADO-REVISTA DE CURRICULUM Y FORMACION DE PROFESORADO},
Year = {2017},
Volume = {21},
Number = {3},
