@article{ WOS:000326563400003,
Author = {Turcan, L'udovit},
Title = {Unpublished Encyclopedia. The Process with Alexander Hirner and his
   Collaborators Started Fifty Five Years Ago},
Journal = {SOCIOLOGIA},
Year = {2013},
Volume = {45},
Number = {5},
Pages = {489-507},
Abstract = {After the Second World War, in countries under the influence of the USSR
   including Czechoslovak Republic many politically motivated monumental
   processes were tried. Many of them began even after illegal actions had
   been revealed because of the personality cult of J.V. Stalin
   notwithstanding official party documents for remedial actions that were
   approved in the Czechoslovak Republic.
   Due to overall cultural sovietization, some branches of science among
   them also sociology were condemned in these ideological fights and
