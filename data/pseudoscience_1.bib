@article{ WOS:000229580100003,
Author = {Grant, DC and Harari, E},
Title = {Psychoanalysis, science and the seductive theory of Karl Popper},
Journal = {AUSTRALIAN AND NEW ZEALAND JOURNAL OF PSYCHIATRY},
Year = {2005},
Volume = {39},
Number = {6},
Pages = {446-452},
Month = {JUN},
Abstract = {Objective:To present a critique of the ideas of Karl Popper, the
   philosopher of science, whose depiction of psychoanalysis as a
   pseudoscience is often used to justify attacks on psychoanalysis.
   Method: Published sources are used to provide a brief intellectual
   biography of Popper, a summary of his concept of science and a summary
   of criticisms of Popper's view of science. His depiction of
   psychoanalysis and Freud's reply are presented. Clinical, experimental
   and neurobiological research which refutes Popper's view is summarized.
   Results: There is a vast scholarly published work critical of Popper's
   falsifiability criterion of science. Less recognized is Popper's
   misunderstanding and misrepresentation of psychoanalysis; his argument
   against it is logically flawed and empirically false. Even if Popper's
   theory of science is accepted, there is considerable clinical,
   experimental and neurobiological research in psychoanalysis which meets
   Popper's criterion of science.
   Conclusion: Attacks on psychoanalysis based on Popper's theory of
   science are ill-founded and reflect inadequate scholarship.},
