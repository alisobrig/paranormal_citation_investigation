@article{ WOS:A1993LN19400002,
Author = {REEVES, TC},
Title = {PSEUDOSCIENCE IN COMPUTER-BASED INSTRUCTION - THE CASE OF LEARNER
   CONTROL RESEARCH},
Journal = {JOURNAL OF COMPUTER-BASED INSTRUCTION},
Year = {1993},
Volume = {20},
Number = {2},
Pages = {39-46},
Month = {SPR},
Abstract = {Contemporary research studies on learner control in computer-based
   instruction constitute `'pseudoscience'' in that these studies fail to
   live up to the theoretical, definitional, methodological, and analytical
   guidelines of the positivist, quantitative paradigm they purport to
   follow. A critique of published learner control studies is presented
   detailing problems with definitions of learner control, theoretical
   foundations, treatment duration, outcome measures, sample sizes, and
   conclusions. Recommendations are made concerning the exploration of
   alternative paradigms for research focused on learner control and other
   aspects of computer-based instruction.},
