@article{ WOS:000475404200016,
Author = {Fasce, Angelo and Pico, Alfonso},
Title = {Conceptual foundations and validation of the Pseudoscientific Belief
   Scale},
Journal = {APPLIED COGNITIVE PSYCHOLOGY},
