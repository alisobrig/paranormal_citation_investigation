Language = {English},
Affiliation = {Robson, SG (Corresponding Author), Univ New South Wales, Kensington, NSW 2052, Australia.
   Martire, Kristy A. A.; Robson, Samuel G. G.; Drew, Manisara; Nicholls, Kate; Faasse, Kate, Univ New South Wales, Kensington, NSW 2052, Australia.},
DOI = {10.3758/s13423-023-02321-2},
EarlyAccessDate = {JUN 2023},
ISSN = {1069-9384},
EISSN = {1531-5320},
Keywords = {Fake news; Analytic thinking; Cognitive reflection test; Intuition;
   Misinformation; Conspiracy theories},
