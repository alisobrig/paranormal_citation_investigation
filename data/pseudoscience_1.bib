@article{ WOS:000397989800001,
Author = {Jakovljevic, Miro and Ostojic, Ljerka},
Title = {SCIENCE AND PSEUDOSCIENCE IN MEDICINE: EVIDENCE-BASED VS.
   EVIDENCE-BIASED MEDICINE},
Journal = {PSYCHIATRIA DANUBINA},
Year = {2016},
Volume = {28},
Number = {2},
Pages = {2-6},
Abstract = {The concept of evidence-based medicine (EBM), as the highest standard of
   health care, came into existence in 1990s to promote a systematic
   approach to helping clinicians in their practice to be guided by the
   best available scientific evidence. However, there has been an
   increasing number of warning reports ``that in modern research,
   misrepresented, false and unuseful findings may be the majority or even
   the vast majority of published research claims In spite of the huge
   scientific progress, pseudoscience and associated evidence biased
   medicine represent a serious threat to the concept of the EBM. Effective
   education in medicine, proper research motivation, sound systems and
   creative thinking and culture of scientific dialogue may significantly
   contribute to better science and evidence-based medicine. The seven key
   words of good science, research and publishing are: integrity,
   motivation, capacity, understanding, knowledge, experience, and
   creativity.},
