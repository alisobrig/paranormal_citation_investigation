@article{ WOS:001089153700007,
Author = {Wilkinson, Lane},
Title = {Conceptual Metaphors in Information Literacy: Reframing the Scholarly
   Conversation as Scholarly Collaboration},
Journal = {LIBRARY QUARTERLY},
Year = {2023},
Volume = {93},
Number = {4},
Pages = {455-478},
Month = {OCT 1},
Abstract = {The ACRL Framework for Information Literacy in Higher Education proposes
   that the metaphor ``scholarship as conversation{''} is an essential
   concept for information literacy-that is, to be information literate,
   one must understand that scholarship is a conversation. This article
   challenges that position. I argue that the conversation metaphor fails
   to capture some of the most salient aspects of contemporary
   scholarship-especially the gatekeeping function of peer review-and
   inadvertently privileges a hermeneutic conception of scholarship that
   emphasizes its process at the expense of its purpose, providing safe
   haven for fringe theories and pseudoscience. The genealogy of the
   conversation metaphor is traced to determine its underlying structure,
   its aptness as a metaphor is considered, and an alternative model of
   scholarship as collaboration is proposed. The goal is to lay bare the
   scholarly conversation's strengths and weaknesses and to demonstrate
   that the metaphors of information literacy are worth critical appraisal.},
