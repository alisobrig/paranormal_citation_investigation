@article{ WOS:000562405200001,
Author = {Moberger, Victor},
Title = {Bullshit, Pseudoscience and Pseudophilosophy},
Journal = {THEORIA-A SWEDISH JOURNAL OF PHILOSOPHY},
Year = {2020},
Volume = {86},
Number = {5},
Pages = {595-611},
Month = {OCT},
Abstract = {In this article I give a unified account of three
   phenomena:bullshit,pseudoscienceandpseudophilosophy. My aims are partly
   conceptual, partly evaluative. Drawing on Harry Frankfurt's seminal
   analysis of bullshit, I give an account of the three phenomena and of
   how they are related, and I use this account to explain what isbadabout
   all three. More specifically, I argue that what is defective about
   pseudoscience and pseudophilosophy is precisely that they are special
