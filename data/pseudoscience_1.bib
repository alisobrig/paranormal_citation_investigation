@article{ WOS:000374078200025,
Author = {Vinny, Pulikottil Wilson and Vishnu, Venugopalan Y. and Lal, Vivek},
Title = {Trends in scientific publishing: Dark clouds loom large},
Journal = {JOURNAL OF THE NEUROLOGICAL SCIENCES},
Year = {2016},
Volume = {363},
Pages = {119-120},
Month = {APR 15},
Abstract = {The world wide web has brought about a paradigm shift in the way medical
   research is published and accessed. The ease with which a new journal
   can be started/hosted by publishing start-ups is unprecedented. The
   tremendous capabilities of the world wide web and the open access
   revolution when combined with a highly profitable business have
   attracted unscrupulous fraudulent operators to the publishing industry.
   The intent of these fraudulent publishers is solely driven by profit
   with utter disregard to scientific content, peer reviews and ethics.
   This phenomenon has been referred to as ``predatory publishing{''}. The
   ``international{''} tag of such journals often betrays their true
   origins.
   The gold open access model of publishing, where the author pays the
   publisher, when coupled with a nonexistent peer review threatens to blur
   the distinction between science and pseudoscience. The average
   researcher needs to be made more aware of this clear and present danger
   to the scientific community. Prevention is better than cure. (C) 2016
   Elsevier B.V. All rights reserved.},
