@article{ WOS:000452916900023,
Author = {Artamonov, Denis and Tikhonova, Sophia},
Title = {Historical pseudo science as a phenomenon of media-sphere},
Journal = {DIALOG SO VREMENEM-DIALOGUE WITH TIME},
Year = {2018},
Number = {65},
Pages = {317-335},
Abstract = {The article deals with the interaction between scientific and
   pseudo-scientific historical knowledge in media sphere. Research
   methodology is based on the combination of the language of the history
   of ideas and that of social constructivism, together with the
   terminology of communication theory. The authors interpret the
   historical and scientific knowledge as an element of the printed media
   sphere, explore its features, leading to the appearance of
   pseudo-scientific knowledge and determine electronic nature of the
   pseudoscience media manipulative techniques. The authors discuss the
   impact of science and pseudo-science to historical memory. As a result,
   they come to the conclusion that a powerful mythologizers potential of
   the pseudoscience does not mean the primacy of its role in shaping of
   the historical memory. In this process the pseudoscience is secondary to
   the science, while science efficiently conducts research and produces
   reliable past for society.},
