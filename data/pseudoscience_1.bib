@article{ WOS:000870669100008,
Author = {De Beer, Josef and Van Wyk, Ben-Erik},
Title = {Learning about Science \& Pseudoscience as Critical Consumers: A
   Classroom Activity on the Rationality of Plant, Medicinal \& Cosmetic
   Products Use},
Journal = {AMERICAN BIOLOGY TEACHER},
Year = {2022},
Volume = {84},
Number = {8},
Pages = {488-495},
Month = {OCT},
Abstract = {In the 21st century, which is often characterized by fake news and
   pseudoscientific claims, it is important that students should engage
   with the tenets of science, and how it differs from pseudoscience. The
   Rationality Index for Plant Use (RIPU) tool, introduced in this article,
   could be used in a problem-based and cooperative learning activity,
   where students explore the tenets of respectively science and
   pseudoscience. During such a learning activity, students will engage
