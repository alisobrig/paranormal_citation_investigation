@article{ WOS:000383254900011,
Author = {Escriba-Sales, Eudald and Cortinas Rovira, Sergi and Alonso-Marcos,
   Felipe},
Title = {Pseudoscience in the media: A case study of homeopathy as covered in the
   Spanish and British press (2009-2014)},
Journal = {PANACEA-BOLETIN DE MEDICINA Y TRADUCCION},
Year = {2015},
Volume = {16},
Number = {42},
Pages = {177-183},
Month = {DEC},
Abstract = {The goal of this article is to analyze Spanish and British newspapers'
   presentation of reports on the safety and efficacy of homeopathy from
   2009 to 2014. To achieve that goal, we selected, classified, and
   performed statistical analysis on the content of news articles published
   in five Spanish and three British newspapers from January 2009 to June
   2014. The study reveals little commitment on the part of the press to
   providing information on homeopathy's safety and efficacy. In order to
   give the public information for informed decision-making, it would be
   desirable to see, on one hand, greater media attention to scientific
   advances and on the other, especially in Spain, greater editorial
   involvement and more solid, evidence-based coverage of science and
   healthcare.},
