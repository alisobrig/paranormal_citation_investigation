Language = {English},
Affiliation = {Held, BS (Corresponding Author), Bowdoin Coll, Dept Psychol, 6900 Coll Stn, Brunswick, ME 04011 USA.
   Held, Barbara S., Bowdoin Coll, Brunswick, ME 04011 USA.},
DOI = {10.1177/09593543221088555},
EarlyAccessDate = {APR 2022},
Article-Number = {09593543221088555},
ISSN = {0959-3543},
