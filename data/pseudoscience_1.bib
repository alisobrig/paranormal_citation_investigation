@article{ WOS:000354005400004,
Author = {Rousseau, Signe},
Title = {The Celebrity Quick-Fix WHEN GOOD FOOD MEETS BAD SCIENCE},
Journal = {FOOD CULTURE \& SOCIETY},
Year = {2015},
Volume = {18},
Number = {2},
Pages = {265-287},
Month = {JUN},
Abstract = {Fad diets are nothing new. But fad diets peddled by celebrities in this
   unique media climate-aptly described by French sociologist Claude
   Fischler as a ``dietary cacophony{''} thanks to its often conflicting
   and therefore confusing-messages are a novelty and, as argued in this
   article, an unfortunate one. From celebrity foodies like Gwyneth Paltrow
   recommending home-made detox plans to television nutritionists
   prescribing alternative therapies for all manner of ailments, celebrity
   diets are a dime a dozen. Not confined to movie stars and television
   personas, even medical doctors can achieve celebrity by advocating the
   opposite of mainstream medical advice. It is a disconcerting trend
   because it reflects a market for quick-fix solutions, often based on
   questionable science, which is then perpetuated as the publication of
   ``success stories{''} functions to disguise anecdotes as data. Building
   on my existing work on attention economics-specifically, key concepts
   like ``bounded rationality{''} and ``docility{''} as manifestations of
   our tendencies to seek external guidance-I argue that this industry
   entails a threat not only to critical and scientific literacy in the
   public at large, but also and more crucially, to the health and
   wellbeing of those out to ``fix{''} themselves.},
