@article{ WOS:000552687700001,
Author = {Denovan, Andrew and Dagnall, Neil and Drinkwater, Ken and Parker, Andrew
   and Neave, Nick},
Title = {Conspiracist beliefs, intuitive thinking, and schizotypal facets: A
   further evaluation},
Journal = {APPLIED COGNITIVE PSYCHOLOGY},
Year = {2020},
Volume = {34},
Number = {6, SI},
Pages = {1394-1405},
Month = {NOV},
Abstract = {This study examined whether thinking style mediated relationships
   between belief in conspiracy and schizotypy facets. A UK-based sample of
   421 respondents completed the Generic Conspiracist Beliefs Scale (GCBS),
   Oxford-Liverpool Inventory of Feelings and Experiences Short (O-Life),
   and measures indexing preferential thinking style (proneness to reality
   testing deficits and Need for Cognition). Path analysis revealed direct
   and indirect relationships between Conspiracy Beliefs and schizotypy
   facets. Unusual Experiences had a direct effect on Conspiracy Beliefs
   and predicted Reality Testing and Need for Cognition. Preferential
   thinking style mediated the schizotypy-belief in conspiracy
   relationship. This pattern of results (higher experiential-based
   processing and lower Need for Cognition) was consistent with intuitive
   thinking. Introverted Anhedonia and Impulsive Nonconformity predicted
   Reality Testing and had indirect effects on Conspiracy Beliefs. Finally,
   Reality Testing predicted Conspiracy Beliefs, whereas Need for Cognition
   did not. These results confirm that cognitive processes related to
   thinking style mediate the schizotypy-conspiracist beliefs relationship.},
