@article{ WOS:000303507700026,
Author = {Vu, Lung and Tun, Waimar and Sheehy, Meredith and Nel, Dawie},
Title = {Levels and Correlates of Internalized Homophobia Among Men Who Have Sex
   with Men in Pretoria, South Africa},
Journal = {AIDS AND BEHAVIOR},
