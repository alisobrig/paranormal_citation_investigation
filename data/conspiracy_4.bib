@article{ WOS:000086726600001,
Author = {Bertheau, G},
Title = {Popular songs and political events: The case of George Chapmans's The
   `Conspiracy and Tragedy of Byron' (1608)},
Journal = {ETUDES ANGLAISES},
Year = {1999},
Volume = {52},
Number = {3},
Pages = {259-274},
Month = {JUL-SEP},
Abstract = {The songs written about the Marshal of Byron (1562-1602)-both French and
   Piedmontese-have not been exploited so far, at least in the way they are
   dealt with in this article. Yet, far from conforming to Henry IV's
   propaganda, they testify to the popularity of the Marshal and to the
   repercussions of his execution and enable us to better understand George
   Chapman's Conspiracy and Tragedy of Byron (1608), in which, although he
   does not exculpate Byron, he denounces an absolutist monarchical system
   that sets little store by justice in the name of the king's glory and
   authority.},
