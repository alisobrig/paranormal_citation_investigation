@article{ WOS:000424570200012,
Author = {Vega Pichaco, Belen},
Title = {THE FIGHT FOR AN AMERICAN DREAM: LINCOLN KIRSTEIN FACING THE RUSSIAN
   BALLET'S CONSPIRACY DURING THE PRE-COLD WAR},
