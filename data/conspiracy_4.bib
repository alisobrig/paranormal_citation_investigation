@article{ WOS:000895812900001,
Author = {Vitriol, Joseph A. and Sandor, Joseph and Vidigal, Robert and Farhart,
   Christina},
Title = {On the Independent Roles of Cognitive \& Political Sophistication:
   Variation Across Attitudinal Objects},
Journal = {APPLIED COGNITIVE PSYCHOLOGY},
Year = {2023},
Volume = {37},
Number = {2, SI},
Pages = {319-331},
Month = {MAR},
Abstract = {People are motivated to maintain consistency between importantly held
   identities, preferences, and judgments. In political contexts, motivated
   reasoning can help explain a wide range of political phenomena,
   including extremism, polarization, and misperceptions. However, recent
   findings in psychology have challenged this account. These perspectives
   emphasize the role of cognitive sophistication (e.g., analytical
   reasoning, numerical literacy) in political attitudes, but differ in
   terms of whether it is expected to attenuate or exacerbate politically
   motivated reasoning and belief in conspiracy theories. Yet prior
   investigations have not examined the relative and independent effects of
   both political and cognitive sophistication. Using data from two
   samples, including one sampled to approximate representativeness in the
   U.S., we demonstrate that both types of sophistication have independent
   and (at times) countervailing effects on belief in COVID-19 conspiracy
   theories and other political attitudes. Our results are critical for
   theories of cognitive sophistication, political cognition, and
   attitudes, and the psychology of conspiracy theories.},
