@article{ WOS:000718328700011,
Author = {Sanchez Arteaga, Juan Manuel},
Title = {FROM GATHERINGS TO CONSPIRACY: THE DISPUTE ABOUT BEAUTY AND ALEJANDRO
   MALASPINA'S DANGEROUS LIAISONS IN MADRID},
Journal = {ASCLEPIO-REVISTA DE HISTORIA DE LA MEDICINA Y DE LA CIENCIA},
Year = {2021},
Volume = {73},
Number = {2},
Month = {JUL-DEC},
Abstract = {This article reviews the period spent by Malaspina in Madrid after his
   ``voyage around the world{''}, until his arrest in November 1795.
   Exploring sources hitherto ignored by Malaspinian historiography, the
   article focuses on Malaspina's participation in a literary dispute which
   took place in the Diario de Madrid, where he published under pseudonym
   four philosophical letters on Beauty. These letters constitute the
   direct antecedent of his Meditation on Beauty, his most personal work,
   written in prison after his arrest. Lastly, the paper analyses
   Malaspina's relationship with his neighbour in Madrid, Fernanda
   O'Connock, Marchioness of Matallana, a woman linked to the political
   opposition to Manuel Godoy and an outstanding conspirator agent against
   the Prime Minister, who very possibly played a much more active role
   than previously recognised in the so-called ``Malaspina conspiracy{''}.},
