@article{ WOS:000855940100001,
Author = {Quinn, Emma K. and Heer, Emily V. and Fazel, Sajjad S. and Peters,
   Cheryl E.},
Title = {The Instagram Infodemic Persists: Extreme Content Escapes Platform's
   Removal Tactics},
