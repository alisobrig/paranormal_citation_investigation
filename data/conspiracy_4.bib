@article{ WOS:001061069400011,
Author = {Oparinde, Kunle},
Title = {INFODEMIC AND MISINFORMATION IN THE ERA OF CORONAVIRUS: An Analysis of
   Selected Social Media Rhetoric from African Audiences},
Journal = {AFRICAN JOURNAL OF RHETORIC},
Year = {2023},
Volume = {15},
Number = {1},
Pages = {322-343},
Month = {JUN},
Abstract = {The COVID-19 pandemic has seen rumours and conspiracy theories overpower
   the truth in many online media platforms across several African
   countries. Just as the coronavirus has travelled widely, misinformation
   has spread widely. Thus, it is important to launch investigations into
   these conspiracy theories to detect them early on, and, as a result,
   implore health practitioners and agencies to be more proactive in
   repelling misinformation while simultaneously providing the general
   populace with purely undiluted information regarding the virus. This
   study draws instances of infodemic and misinformation from social media
   platforms by reviewing and analysing these texts and the resulting
   implications if the misinformation continues to gain popularity. This
   study is done purely to analyse the instances of the `infodemic' to
   check social perceptions and with the notion that individuals begin to
   take responsibility for the information they consume and share. The
   study discovers three categories within the COVID-19 `infodemic,' namely
   the discourse of conspiracy, the discourse of denial, and the remedial
   discourse. Notably, the study encourages users of online media
   platforms, as well as people generally, to consider the notion of
   authority before relaying or sharing information found on social media.
   This will assist the public to be cautious and vigilant against false
   information being peddled as original.},
