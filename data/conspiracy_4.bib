@article{ WOS:000361249000004,
Author = {Lyandres, Semion},
Title = {Conspiracy and Ambition in Russian Politics before the February
   Revolution of 1917: The Case of Prince Georgii Evgen'evich L'vov},
Journal = {JOURNAL OF MODERN RUSSIAN HISTORY AND HISTORIOGRAPHY},
Year = {2015},
Volume = {8},
Number = {1},
Pages = {99-133},
Abstract = {This article uses new and previously known but under-exploited sources
   to reexamine the significance of pre-revolutionary conspiracies centered
   around Prince G.E. L'vov for the liberals' dilemma about the leadership
   and form of a popular cabinet. The article sheds new light on L'vov's
   efforts and motives to organize these plots, challenges his traditional
   depiction as a `soft' and ineffectual politician, and questions his
   long-assumed rivalry with the Duma President M.V. Rodzianko for the
   premiership in a post-Nikolaevan government.},
