@article{ WOS:000965579500009,
Author = {Ugolini, Beatrice},
Title = {Coronavirus and New World Order},
Journal = {RELIGIONI E SOCIETA-RIVISTA DI SCIENZE SOCIALI DELLA RELIGIONE},
Year = {2022},
Volume = {37},
Number = {103},
Pages = {72-78},
Month = {MAY-AUG},
Abstract = {Based on the consideration of some of the most relevant websites and
   books, believed to be conspiracy-oriented, the paper aims to analyze how
   the Coronavirus outbreak influenced the conspiracy narrative, which
   involves the establishment of a New World Order by elites members of
   esoteric orders. It will also consider a recent conspiracy declination,
   the theory << QAnon >>, spread just following the appearance of
   Covid-19, which places the former President Trump at the head of a
   network fighting the New World Order. Finally, an important question
   must be asked: is it possible to identify in the conspiracy theories,
   increased by the pandemic, a nucleus, however controversial, of truth ?
   In this regard, some possible levels of interpretation of conspiracy
   theories are proposed.},
