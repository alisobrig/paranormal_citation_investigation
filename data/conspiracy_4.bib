@article{ WOS:000990228000001,
Author = {Algavi, Leila and Volkova, Irina and Kovalev, George and Budtsov,
   Grigory},
Title = {Qanon as a Transmedia Storytelling},
Journal = {MEDIA EDUCATION-MEDIAOBRAZOVANIE},
Year = {2023},
Number = {1},
Pages = {3-9},
Abstract = {The communicative nature of QAnon's most ambitious contemporary
   conspiracy theory is the focus of this study. According to some
   preliminary speculation, QAnon's incredible popularity lies in its
   superficial resemblance to an alternate reality game and a live-action
   role-playing game. This is true, but the authors present a system of
   arguments proving that QAnon is primarily a transmedia project with all
   its inherent properties. Analysis of the QAnon open narrative system
