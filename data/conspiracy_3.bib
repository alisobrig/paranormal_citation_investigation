@article{ WOS:000398715300001,
Author = {Merrick, Jeffrey},
Title = {Claude Amable de Cohade de La Garderie: Family Conflict and Political
   Conspiracy under Louis XV},
Journal = {CANADIAN JOURNAL OF HISTORY-ANNALES CANADIENNES D HISTOIRE},
Year = {2017},
Volume = {52},
Number = {1},
Pages = {1-28},
Month = {SPR-SUM},
Abstract = {The merchant Claude Amable de Cohade de La Garderie, arrested and
   imprisoned in 1773 as a result of family disputes, revealed a Jesuit
   conspiracy to destroy the parlements and the Bourbon dynasty. His
   dossier in the Archives de la Bastille contains three hundred pages,
   including two dozen letters and memoires in which he summarized his
   biography, recounted his conflicts with his wife, his brother, and his
   lawyer, all of whom claimed that he was demented and dangerous, and
   requested assistance from the lieutenant general of police and other
   notables. This strange case illustrates the ways in which French
   subjects could connect their own lives with public affairs and the ways
   in which that connection, even if constructed out of loyalty to the
   king, could backfire on them.
   Le marchand Claude Amable de Cohade de La Garderie, arrete et emprisonne
   en 1773 en resultat de conflits familiaux, a revele un complot jesuite
   en vue de detruire les parlements et la dynastie des Bourbon. Son
   dossier aux Archives de la Bastille contient trois cents pages, y inclus
   vingt-quatre lettres et memoires resumant sa biographie, ainsi que ses
   conflits avec son epouse, son frere et son avocat. Ils l'ont tous
   declares dangereux et dement. De plus, dans ces documents il a demande
   l'aide du lieutenant-general de la police et d'autres notables. Cette
   affaire curieuse montre une facon dont les sujets francais pouvaient
   faire le lien entre leur propre vie et les affaires publiques, de meme
   que la facon dont ce lien, meme s'il etait fonde sur la fidelite au roi,
   risquait de se retourner contre lui.},
