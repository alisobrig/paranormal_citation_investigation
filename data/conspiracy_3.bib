@inproceedings{ WOS:000189438700057,
Author = {Lorenz, U},
Editor = {Monien, B and Feldmann, R},
Title = {Parallel controlled conspiracy number search},
Booktitle = {EURO-PAR 2002 PARALLEL PROCESSING, PROCEEDINGS},
Series = {Lecture Notes in Computer Science},
Year = {2002},
Volume = {2400},
Pages = {420-430},
Note = {8th International Euro-Par Conference on Parallel Processing, PADERBORN,
   GERMANY, AUG 27-30, 2002},
Abstract = {Tree search algorithms play an important role in many applications in
   the field of artificial intelligence. When playing board games like
   chess etc., computers use game tree search algorithms to evaluate a
   position. In this paper, we present a procedure that we call Parallel
   Controlled Conspiracy Number Search (Parallel CCNS). Briefly, we
   describe the principles of the sequential CCNS algorithm, which bases
   its approximation results on irregular subtrees of the entire game tree.
   We have parallelized CCNS and implemented it in our chess program
