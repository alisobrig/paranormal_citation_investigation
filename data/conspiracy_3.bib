@article{ WOS:000535682400002,
Author = {Clifford, Scott and Kim, Yongkwang and Sullivan, Brian W.},
Title = {AN IMPROVED QUESTION FORMAT FOR MEASURING CONSPIRACY BELIEFS},
Journal = {PUBLIC OPINION QUARTERLY},
Year = {2019},
Volume = {83},
