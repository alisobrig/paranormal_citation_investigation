@article{ WOS:A1995RX94200003,
Author = {CHRISTOFFERSON, JM},
Title = {OBSTACLES TO CIVIL-RIGHTS - THE INTRACORPORATE CONSPIRACY DOCTRINE
   APPLIED TO 42-USC-1985(3)},
Journal = {UNIVERSITY OF ILLINOIS LAW REVIEW},
Year = {1995},
Number = {2},
Pages = {411-440},
Abstract = {In 1871, the Forty-Second Congress passed the Ku Klux Klan Act to combat
   the widespread violence and racial hatred that plagued the South after
   the Civil War. The Act is embodied today in 42 U.S.C. 1985(3) which, as
   a conspiracy statute, still provides an important means of addressing
   civil rights violations. Although 1985(3)'s language is broad enough to
   cover the more modern, subtle forms of prejudice, courts have curtailed
   1985(3)'s effectiveness through several judicial restrictions. One of
   the more recent restrictions that plaintiffs have faced in 1985(3)
   actions is the application of the intracorporate conspiracy doctrine to
   civil rights litigation. The basic premise of the doctrine is that a
   corporation cannot conspire with itself; that is, if the alleged
   conspirators are agents of the same company, the doctrine holds that
   only the principal can be held liable, and therefore, the plurality of
   actors necessary to form a conspiracy is lacking. This note challenges
   the appropriateness of applying the intracorporate conspiracy doctrine
   in the civil rights context. After explaining the history and purposes
   of 1985(3), the author discusses the development of the intracorporate
   conspiracy doctrine in the antitrust area and critiques the
   justifications offered for allowing the doctrine to bar 1985(3) claims.
   The author concludes that the intracorporate conspiracy doctrine has no
   role in 1985(3) actions and suggests an approach to liability that
   better serves the section's purpose.},
