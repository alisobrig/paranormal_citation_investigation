@article{ WOS:000327440300009,
Author = {Ford, Chandra L. and Wallace, Steven P. and Newman, Peter A. and Lee,
   Sung-Jae and Cunningham, William E.},
Title = {Belief in AIDS-Related Conspiracy Theories and Mistrust in the
   Government: Relationship With HIV Testing Among At-Risk Older Adults},
