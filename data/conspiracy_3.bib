@article{ WOS:000362670500012,
Author = {Van Prooijen, Jan-Willem and Acker, Michele},
Title = {The Influence of Control on Belief in Conspiracy Theories: Conceptual
   and Applied Extensions},
Journal = {APPLIED COGNITIVE PSYCHOLOGY},
Year = {2015},
Volume = {29},
