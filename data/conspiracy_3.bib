@article{ WOS:000184891900002,
Author = {Plaag, EW},
Title = {New York's 1741 slave conspiracy in a climate of fear and anxiety},
Journal = {NEW YORK HISTORY},
Year = {2003},
Volume = {84},
Number = {3},
Pages = {275-299},
Month = {SUM},
Abstract = {While much has been written about the 1741 New York Slave Conspiracy,
   very little attention has been paid to the pressures on New Yorkers in
   the years preceding 1741 that compelled them to accept rumors of
   conspiracy so willingly. In particular, many of the participants in the
   1741 investigation had themselves participated-or had direct familial
   links to those who had-in the investigation of the 1712 New York Slave
   Insurrection, in which conspirators had followed a script for rebellion
   with many similarities to that purportedly uncovered in 1741. Eric W.
   Plaag is a doctoral candidate in history at the University of South
   Carolina.},
