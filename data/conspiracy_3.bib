@article{ WOS:000315172700009,
Author = {Ditrych, Ondrej},
Title = {`International Terrorism' as Conspiracy: Debating Terrorism in the
   League of Nations},
Journal = {HISTORICAL SOCIAL RESEARCH-HISTORISCHE SOZIALFORSCHUNG},
Year = {2013},
Volume = {38},
Number = {1, SI},
Pages = {200-210},
Abstract = {``Internationaler Terrorismus' als Verschworung: Debatten fiber
   Terrorismus im Volkerbund{''}. This paper aims at historicizing
   terrorism by focusing attention on how the current dominant discursive
   patterns, constituent elements of the `global terrorism' dispositive,
   have come to existence. It brings to the fore the first robust
   international debate on terrorism which took place in the League of
   Nations following the Marseilles assassination (1934) and shows its
   conspiratorial features, many of which are detectable in the discourse
   of terrorism among states after 9/11.},
