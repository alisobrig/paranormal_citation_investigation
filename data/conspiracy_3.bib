@article{ WOS:000740712500013,
Author = {Feuillet, Lucia},
Title = {Contemporary Conspiracy Fictions. Three Short Stories From <i>La musica
   interior de los leones</i>},
Journal = {LITERATURA-TEORIA HISTORIA CRITICA},
Year = {2022},
Volume = {24},
Number = {1},
Pages = {321-338},
Month = {JAN-JUN},
Abstract = {This paper analyzes three short stories from La Musica Interior de los
   Leones (2019), by Martin Cristal. It focuses on the conspiratorial
   nature of current narrative fictions from the perspective of dialectical
   hermeneutics. In the imaginary worlds of ``Vivir A La Sombra{''},
   ``Farmagedon{''}, and ``Ofrenda{''}, the detective genre expands to
   question social phobias, rewrites the coordinates of terror, and extends
   uncertainty to scientific paradigms through the allusion to science
   fiction. Thus, the conspiracy is drawn from the discursive organization
   as a network of networks, a privileged, invisible, and conflictive
   totalizing form that allows the nuclei of domination to be read.},
