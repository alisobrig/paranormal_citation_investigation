@article{ WOS:000084678100003,
Author = {Campbell, C},
Title = {Two steps backwards: The Criminal Justice (Terrorism and Conspiracy) Act
   1998},
Journal = {CRIMINAL LAW REVIEW},
Year = {1999},
Pages = {941-959},
Month = {DEC},
Abstract = {This article reviews the main provisions of the Criminal Justice
   (Terrorism and Conspiracy) Act 1998. It argues that the measures
   introduced by the Act to combat terrorism following the Omagh atrocity
   are likely to prove ineffective and that convictions based on these
   measures may be incompatible with human rights legislation. The
   extra-territorial extension of conspiracy is overbroad and should not
   have been included in this Act.},
