@article{ WOS:000279387000008,
Author = {Erutku, Can and Hildebrand, Vincent A.},
Title = {Conspiracy at the Pump},
Journal = {JOURNAL OF LAW \& ECONOMICS},
Year = {2010},
Volume = {53},
Number = {1},
Pages = {223-237},
Month = {FEB},
Abstract = {On the basis of evidence of price-fixing, in May 2006 the Canadian
   Competition Bureau targeted retail gasoline outlets in some local
   markets in the province of Quebec. In June 2008, criminal charges were
   filed against many individuals and companies operating in those local
   markets. We employ a differences-indifferences approach to determine
   whether the public announcement of the antitrust investigation triggered
   a reaction in one of the targeted markets. We find that the price of
   gasoline in the targeted market fell by 1.75 cents per liter after the
   public announcement of the investigation. We also briefly discuss how
   well the Stiglerian theory of collusion performs in this real-world
   conspiracy.},
