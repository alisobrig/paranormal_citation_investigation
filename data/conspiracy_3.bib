@article{ WOS:000462338000002,
Author = {Soteras, Eva},
Title = {THE POLITICAL AND RELIGIOUS STAKES OF CONSPIRACY IN THE POST-MODERN ERA},
Journal = {SOCIETES},
Year = {2018},
Volume = {142},
Number = {4},
Pages = {7-18},
Abstract = {Considered as fascinating by the public but worrying by the authorities,
   conspiracy theories are at the heart of the contemporary collective
   imaginary. Established by various conspiracy leaders who are seen as
   prophets, but also by ``truthers{''}-anonymous internet users or amateur
   investigators-conspiracy theories proliferate in the public space and
   continue to draw in more and more followers. Driven by a collective
   search for meaning and a growing distrust of the authorities and
   official explanations, global conspiracy theories have become
   politico-religious myths whose reasons and degrees of adherence, as well
   as the profiles of its believers, are growing in number and variety.},
