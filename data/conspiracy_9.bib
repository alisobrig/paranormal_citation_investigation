   Hagan, John, Northwestern Univ, Amer Bar Fdn, Evanston, IL 60208 USA.
   McCarthy, Bill, Rutgers Univ Newark, Newark, NJ USA.
   Herda, Daniel, Merrimack Coll, N Andover, MA 01845 USA.},
DOI = {10.1177/13624806221122610},
EarlyAccessDate = {OCT 2022},
ISSN = {1362-4806},
EISSN = {1461-7439},
Keywords = {crimes; politics; privilege},
