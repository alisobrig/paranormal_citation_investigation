@article{ WOS:000314419800009,
Author = {Ball, Simon},
Title = {THE ASSASSINATION CULTURE OF IMPERIAL BRITAIN, 1909-1979},
Journal = {HISTORICAL JOURNAL},
Year = {2013},
Volume = {56},
Number = {1},
Pages = {231-256},
Month = {MAR},
Abstract = {The importance of political assassination lies in the response of the
   state. That response takes place within a specific culture. This article
   analyses the assassination culture of the British imperial state. Most
   studies of assassination in British history concentrate on specific
   events. On the basis of detailed archival investigation the current
   article argues that long-term trends are discernible, in particular that
   the imperial state had a recurrent reflex in characterizing the
   conspiracies that threatened it. This reflex, in turn, governed the
   nature of the response to assassination.},
