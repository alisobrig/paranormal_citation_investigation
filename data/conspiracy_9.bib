@article{ WOS:000252053200006,
Author = {Meder, Theo},
Title = {Modem exempla: Crop circle tales in the new age era},
Journal = {FABULA},
Year = {2007},
Volume = {48},
Number = {3-4},
Pages = {281-299},
Note = {Symposium on Narratives, Roles and Beliefs in the New Age Era, Meertens
   Inst, Amsterdam, NETHERLANDS, FEB 25, 2005},
Abstract = {One of the New Age phenomena is the cult that believes that crop circles
   (mainly) have an extraterrestrial or supernatural origin, and that the
   formations contain messages from beyond or above. One of the effective
   tools for initiation, persuasion and conversion is storytelling. More
   than contemporary legends the stories about crop circles look like
   traditional exempla, illustrating a spiritual (and most of the time
   positive) truth. The narratives can provide building blocks for an
   entire cosmology, ranging from human creation by godlike aliens to
   apocalyptic times in the near future. The accumulation of stories can
   determine people's perception and behaviour in daily life, and
   eventually lead to the belief in secret conspiracies to keep the general
   public ignorant about the truth.},
