@article{ WOS:000444434400007,
Author = {Galletti, Marina},
Title = {Story of a Secret Society},
Journal = {THEORY CULTURE \& SOCIETY},
Year = {2018},
Volume = {35},
Number = {4-5, SI},
Pages = {135-154},
Month = {JUL-SEP},
Abstract = {This article aims to retrace the history of the Acephale secret society
   and its role in the development of the work of Bataille, notably the
   unfinished project of the Atheological Summa (Somme atheologique). Based
   on sociological notions of the secret society' and the society of men',
   it updates the dual aspects of Acephale: a diurnal or political' aspect
   constituted by the publication of the journal Acephale, and afterwards
   by the public activity of the College of Sociology; and a nocturnal or
   religious side, as evidenced by the activity of the secret society
   itself, an activity aiming to strengthen the communitarian link amongst
   the followers, and to open them up to what Caillois would call a broader
   conspiracy'.},
