@article{ WOS:000429309000008,
Author = {Kessareas, Efstathios},
Title = {The Greek debt crisis as theodicy: Religious fundamentalism and
   socio-political conservatism},
Journal = {SOCIOLOGICAL REVIEW},
Year = {2018},
Volume = {66},
Number = {1},
Pages = {122-137},
Month = {JAN},
Abstract = {This article analyzes two core ultra-conservative religious frames that
   were employed for the interpretation of the Greek debt crisis, namely
   the crisis as a pedagogical punishment from God and as a global
   conspiracy of the arch-enemies of the Greek nation. Further, it analyzes
   the corresponding proposed remedies, namely an attitude of repentance
   and an attitude of dynamic defense of traditional values. Finally, the
   article explains the seemingly paradoxical attitude of the Greek
   Orthodox fundamentalists as they both call for resistance and
   simultaneously condemn the anti-austerity movement of the time. In doing
   so, the article underscores the highly ideological character of
   theodicy, since it mobilizes religio-moral and cultural symbolic
   resources for the legitimization of power relations.},
