@article{ WOS:000420726900006,
Author = {Villalba Alvarez, Joaquin},
Title = {Boletum medicatum. The mushroom that killed the emperor Claudius},
Journal = {TEJUELO-DIDACTICA DE LA LENGUA Y LA LITERATURA},
Year = {2009},
Volume = {5},
Number = {1},
Pages = {71-84},
Month = {JUN},
Abstract = {In the course of the centuries, many pages have been written on the
   circumstances surrounding the death of the Roman Emperor Claudius (10 BC
   - 54 AD). General agreement among ancient historians who relate us the
   story - Tacitus, Suetonius, Cassius Dio-, as well as the rest of
   scattered references in Latin literature induce us to think his death
   was due to his fourth wife's conspiracy, his niece Agrippina, who served
   him a dish with previously poisoned mushrooms. Nevertheless, some of the
   scholars of our time, Grimm-Samuel for example, have considered the
   hypothesis that Claudius died because he ingested poisonous mushrooms
   ({''}either through criminal intent, or by sheer accident{''}), more
   specifically Amanita phalloides. Starting from different arguments, we
   offer our own point of view, which does not differ much from the ancient
   writers' opinion.},
Publisher = {JUNTA EXTREMADURA, CONSEJERIA EDUCACION},
