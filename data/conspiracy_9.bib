@article{ WOS:000421640200009,
Author = {Crewe, Emma},
Title = {Doing development differently: Rituals of hope and despair in an INGO},
Journal = {DEVELOPMENT IN PRACTICE},
Year = {2014},
Volume = {24},
Number = {1},
Pages = {91-104},
Abstract = {Through an anthropological lens, using examples from working in an
   international NGO, I explore how and why a group of development workers
   navigated the coercive practices of aid in ways that benefitted their
