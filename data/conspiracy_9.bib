@article{ WOS:000937281400001,
Author = {Solari, Stefano},
Title = {Can a Catholic be liberal? Roman Catholicism and liberalism in a
   political economy perspective (1823-1971)},
Journal = {JOURNAL OF PHILOSOPHICAL ECONOMICS},
Year = {2023},
Volume = {16},
Pages = {1-31},
Abstract = {In the nineteenth century, Enlightenment philosophy and modern political
   thought found tough opposition in the Roman Catholic Church. Liberalism
   was associated with Free Masons conspiracy and revolutionary intent.
   Nonetheless, liberalism and political economy induced the Church to
   develop some theoretical analysis and specific theoretical positions in
   terms of social philosophy and social economics. This paper presents an
   analysis of encyclical letters and writings of Catholic scholars, to
   elaborate on the theoretical points used to contrast liberalism.
   Compromises, as well as turning points in the evolution of the Catholic
   position, are investigated. Lastly, the epistemological and historical
   reasons for the affinity of Roman Catholicism with ethical liberalism
   and the limits of this similarity are discussed.},
