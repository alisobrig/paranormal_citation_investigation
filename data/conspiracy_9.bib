@article{ WOS:000222482200003,
Author = {Okines, AWR},
Title = {Why was there so little government reaction to the Gunpowder Plot?
   (Political pragmatism, propaganda and polemic within the religious
   landscape of Jacobean England)},
Journal = {JOURNAL OF ECCLESIASTICAL HISTORY},
Year = {2004},
Volume = {55},
Number = {2},
Pages = {275-292},
Month = {APR},
Abstract = {This article rejects the approach that treats the Gunpowder Plot as a
   discrete episode. The plot is better understood when examined in
   parallel with the period after November 1605; the surprising leniency
   shown by the Jacobean government towards English Catholics destroys the
   motives upon which conspiracy theories are based. This article
   demonstrates that Robert Cecil, earl of Salisbury, supported King James'
   toleration since both wished to preserve domestic stability and peace
   with Spain. The assassination of King Henri IV of France in 1610 did
   more to jeopardise toleration than did the Gundpowder Plot, despite the
   latter's profound impact on the English popular consciousness.},
