@article{ WOS:000262027700002,
Author = {Caillou, Francois},
Title = {Development and setbacks of the League in Tours (1576-1589)},
Journal = {ANNALES DE BRETAGNE ET DES PAYS DE L OUEST},
Year = {2008},
Volume = {115},
Number = {4},
Pages = {31-53},
Month = {DEC},
Abstract = {In 1589, as France was being devastated by the civil war, Henri III
   chose to make Tours the capital of his kingdom. Until 1594, the city
   would be used as a rear base by Henri IV and as a refuge by the
   opponents of the League. All the same, a powerful movement of Leaguers
   developed there from 1584 onwards. In the course of 1589, on three
