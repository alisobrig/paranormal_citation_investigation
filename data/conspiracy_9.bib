@article{ WOS:000223210000001,
Author = {Buchmüller, W and Di Bari, P and Plümacher, M},
Title = {Some aspects of thermal leptogenesis -: art. no. 105},
Journal = {NEW JOURNAL OF PHYSICS},
Year = {2004},
Volume = {6},
Month = {AUG 10},
Abstract = {Properties of neutrinos may be the origin of the matter-antimatter
   asymmetry of the universe. In the see-sawmodel for neutrino masses, this
   leads to important constraints on the properties of light and heavy
   neutrinos. In particular, an upper bound on the light neutrino masses of
   0.1 eV can be derived. We review the present status of thermal
   leptogenesis with emphasis on the theoretical uncertainties and discuss
   some implications for lepton and quark mass hierarchies, CP violation
   and dark matter. We also comment on the `leptogenesis conspiracy', the
   remarkable fact that neutrino masses may lie in the range where
   leptogenesis works best.},
