@article{ WOS:000904719000001,
Author = {de Ridder, Jeroen},
Title = {Online Illusions of Understanding},
Journal = {SOCIAL EPISTEMOLOGY},
Year = {2022},
Month = {2022 DEC 29},
Abstract = {Understanding is a demanding epistemic state. It involves not just
   knowledge that things are thus and so, but grasping the reasons why and
   seeing how things hang together. Understanding, then, typically requires
   inquiry. Many of our inquiries are conducted online nowadays, with the
   help of search engines, forums, and social media platforms. In this
   paper, I explore the idea that online inquiry easily leads to what I
   will call online illusions of understanding. Both the structure of
   online information presentation (with hyperlinks, shares, retweets,
   likes, etc.) and the operation of recommender systems and the like make
   it easy for people using them to form the impression that they are
   conducting inquiry responsibly, whereas they are in fact fed with
   irrelevant information, or, even worse, falsehoods, misinformation,
   disinformation, or outright conspiracy theories.},
