@article{ WOS:001027576800012,
Author = {Szwako, Jose},
Title = {Denialism, Anti-Globalism and the Defense of Freedom in Contemporary
   Brazilian \&DLANGBRAC; reactionism \&drangbrac;},
Journal = {BRESIL-S},
Year = {2023},
Volume = {23},
Abstract = {This article analyzes the weight of gender metaphors in the production
   Brasil sem medo {[}Brazil without fear], an ultraconservative Brazilian
   newspaper intellectually aligned to Olavo de Carvalho. Starting from the
   distinction between conservatism and \& DLANGBRAC; reactionarism \&
   drangbrac;, the analysis shows that its editorial line was directed
   against traditional media, scientific consensus, and sanitary measures.
   The text also focused on the highly gendered grammar that forms the
   rhetorical basis of the UN agencies denunciation. A close examination of
   this doctrine exposes, in an unprecedented way, the reactionary ideal of
   freedom that is at the center of the struggles and strategies of \&
   DLANGBRAC; reactionarism \& drangbrac; in Brazil today.},
