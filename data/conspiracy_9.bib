@article{ WOS:000350977000002,
Author = {Janzen, David},
Title = {`What he did for me': David's Warning about Joab in 1 Kings 2.5},
Journal = {JOURNAL FOR THE STUDY OF THE OLD TESTAMENT},
Year = {2015},
Volume = {39},
Number = {3},
Pages = {265-279},
Month = {MAR},
Abstract = {This article argues that David orders Solomon in 1 Kgs 2.5-6 to kill
   Joab because of Joab's pattern of assassinating rivals in order to
   benefit his master. David's point in 2.5 is that Joab killed Abner and
   Amasa, two potential enemies of David, without warning, and since Joab
   supported Solomon's brother's claim to the throne, Solomon might be his
   next victim. The Septuagint and the Lucianic recension have altered the
   more difficult text that appears in the Masoretic text in order to make
   it seem as if Joab and harmed David, and so provide a legal and
   religious basis for David and Solomon's conspiracy to kill Joab not
   present in the original text.},
