@article{ WOS:000345885000005,
Author = {Etcheto, Henri and Jougleux, Francois},
Title = {THE POLITICAL SPRINGS OF A HISTORICAL FALSIFICATION: CN. DOMITIUS
   AHENOBARBUS AND THE IDES OF MARCH},
Journal = {HISTORIA-ZEITSCHRIFT FUR ALTE GESCHICHTE},
Year = {2015},
Volume = {64},
Number = {1},
Pages = {106-130},
Abstract = {Are-examination of the record leads to establish the guilt of Cn.
   Domitius Ahenobarbus in the conspiracy of Ides of March, a question
   which has been so long debated. The Ciceronian testimony seems
   indisputable about it. Convicted as one of the murderers of Caesar under
   the lex Pedia, Ahenobarbus owed his exculpation and restitutio only to
   an opportune alliance with Antonius who sought supporters after the
   Perusine War. Asinius Pollio himself negotiated this arrangement and
   reinstatement and eventually obtained the reluctant acceptance of
   Octavian during the Brundisium Agreement. Then, Pollio promoted this
   version in his historical account later used by Suetonius and Appian,
   thus reproducing the truth now official about Ahenobarbus' innocence.},
