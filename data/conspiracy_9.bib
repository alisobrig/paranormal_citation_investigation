@article{ WOS:000210748600004,
Author = {Kapur, Jyotsna},
Title = {Fear on the Footsteps of Comedy: Childhood and Paranoia in Contemporary
   American Cinema},
Journal = {VISUAL ANTHROPOLOGY},
Year = {2008},
Volume = {22},
Number = {1},
Pages = {44-51},
Abstract = {Beginning in the 21st century the American conspiracy thriller, one of
   the most popular of contemporary genres, became preoccupied with
   scenarios of protagonists threatened with the loss of children. What had
   until recently been the subject of light-hearted family films, i.e., on
   the growing up of children and the growing down of adults, reappeared
   here as a reason for dread. Tracing the very gendered nature of this
   fear in the contemporary thriller, this paper contextualizes the
   phenomenon in the culture of a radicalized globalized capitalism and an
   outcome of the neo-liberal turn since the 1980s.},
