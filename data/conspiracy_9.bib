@article{ WOS:000889341700004,
Author = {Camere Figueroa, Elian Mauricio},
Title = {AGAIN, THE CRIMINAL LAW OF THE ENEMY: A NECESSITY TO ENSURE THE
   CONDITIONS OF LEGALITY},
