@article{ WOS:000926130000009,
Author = {Richter, Andrei},
Title = {Is there an international right to truth in the post-truth world?},
Journal = {CATALAN JOURNAL OF COMMUNICATION \& CULTURAL STUDIES},
Year = {2022},
Volume = {14},
Number = {2},
Pages = {317-323},
Month = {OCT},
Abstract = {This article raises questions on the political and legal side of lies
   and truths: (1) is there a human right to (know) the truth? (2) Is there
   a human right to (disseminate) lies? They are important issues today as
   the governments in too many parts of the world aim to suppress political
   voices by ostracizing and outlawing disinformation in communications and
   the media. So far this happens in the context of countering COVID-19
   conspiracies and anti-vax movements, but generally designed to tackle
   any socially important information. On the other hand, the world indeed
   stands bewildered at the current outbursts of false news and the
   governments search for ways and means to diminish their social harm. The
   article provides a viewpoint for what the international law says on lies
   and truths in global affairs.},
