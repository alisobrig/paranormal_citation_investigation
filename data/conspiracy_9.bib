@article{ WOS:000504129200003,
Author = {Maritz, Nicholas},
Title = {THE TRUTH ABOUT OVAMBO WOMEN'S COPPER ANKLE RINGS},
Journal = {SOUTH AFRICAN ARCHAEOLOGICAL BULLETIN},
Year = {2019},
Volume = {74},
Number = {209},
Pages = {16-24},
Month = {JUN},
Abstract = {From as early as 1834, private Portuguese traders from southern Angola
   traded heavy copper ankle rings with Ovambo Kings. These rings prevented
   women captured from neighbouring Ovambo Kingdoms from escaping their
   domestic slavery. However, various ethnographic accounts fail to mention
   the role of these rings in the wider practice of slavery and slave
   trafficking. They describe the function of the ankle rings as portraying
   a woman's marital status and the wealth of her husband. This convenient
   conspiracy of silence suited both the Ovambo Kings and their colonial
   trading partners. It also suited the Finnish missionaries who entered
   Ovamboland in 1870 to convert the Ovambo to Christianity since the
   missionaries' initial anti-slavery position made their tenure
   precarious. More recent colonial descriptions of Ovambo culture
   persisted in explaining the copper ankle rings purely as items of
   prestige, and erroneously ascribed their manufacture to the Ovambo
   themselves.},
