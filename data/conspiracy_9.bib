@article{ WOS:000496345000012,
Author = {Ospovat, Kirill},
Title = {Execution of the Author: The Trial of AP Volynskii, Absolutism, and the
   Problem of Political Literature in 1740},
Journal = {NOVOE LITERATURNOE OBOZRENIE},
Year = {2018},
Number = {151},
Pages = {143-163},
Abstract = {This article discusses the historico-cultural contexts and conceptual
   implications of the trial of A.P. Volynskii (1740), a grandee under
   Empress Anna Ivanovna, executed on the charge of a state conspiracy. The
   official narrative of Volynskii's supposed crime was founded not so much
   on Volynskii's actions, or even his ideas, as on textual strategies that
   came across as being too bold - on his claims to authorship and
   authority. Both Volynskii's texts and the royal response to them were
   inscribed in the context of court literature as it emerged in Russia,
   the poetico-political practices and interpretations of the device of
   allegory, as well as in theoretical discussions on the permissibility or
