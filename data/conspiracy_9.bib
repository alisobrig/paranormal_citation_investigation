@article{ WOS:A1996WM08500012,
Author = {Sawer, M and Jupp, J},
Title = {The two-way street: Government shaping of community-based advocacy},
Journal = {AUSTRALIAN JOURNAL OF PUBLIC ADMINISTRATION},
Year = {1996},
Volume = {55},
Number = {4},
Pages = {82-99},
Month = {DEC},
Abstract = {The interaction between government and community-based advocacy
   organisations is becoming complex and highly structured. While some
   analysts seek to explain such interplay within neo-Marxist or public
   choice frameworks, we argue the relationship is best understood as a
   two-way street, fill of tensions but neither a conspiracy against the
   public interest nor parr of a larger design to deradicalise social
   movements. Drawing on a wide range of Australian examples, but focusing
   in particular on peak women's and ethnic communities' organisations, we
   explore how social movements have been able to exert independent
   influence on the policy agenda.},
