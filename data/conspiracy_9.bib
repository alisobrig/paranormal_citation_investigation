@article{ WOS:000460960300015,
Author = {Cavojova, Vladimira and Secara, Eugen-Calin and Jurkovic, Marek and
   Srol, Jakub},
Title = {Reception and willingness to share pseudo-profound bullshit and their
   relation to other epistemically suspect beliefs and cognitive ability in
   Slovakia and Romania},
Journal = {APPLIED COGNITIVE PSYCHOLOGY},
Year = {2019},
Volume = {33},
Number = {2},
Pages = {299-311},
Month = {MAR-APR},
Abstract = {Propensity to judge randomly generated, syntactically correct (i.e.,
   bullshit) statements as profound is associated with a variety of
   conceptually relevant variables (e.g., intuitive cognitive style and
   supernatural beliefs). Besides generalizing these findings to a
   different cultural setting, we examined the relationships to sharing the
   bullshit on social media. Rating nonsense as profound was associated
   with a lower cognitive ability; a stronger belief in the paranormal,
   alternative medicine, and conspiracies; and ontological confusion. The
   more profound a statement was rated to be, the more likely it was to be
   shared, and propensity for sharing bullshit was predicted by ontological
   confusion and religious beliefs. Bullshit receptivity and sharing may be
   closely related to several dimensions of epistemically suspect beliefs;
   people with these propensities are relatively open to vague statements
   resembling New Age spirituality.},
