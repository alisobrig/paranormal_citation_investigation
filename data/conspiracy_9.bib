@article{ WOS:000288686100002,
Author = {Halberstadt-Freud, Hendrika C.},
Title = {Split loyalties of third generation children of Nazis: The case of Lisa},
Journal = {PSYCHE-ZEITSCHRIFT FUR PSYCHOANALYSE UND IHRE ANWENDUNGEN},
Year = {2011},
Volume = {65},
Number = {3},
Pages = {218-238},
Month = {MAR},
Abstract = {Split loyalties of third generation children of Nazis: The case of Lisa.
   - The subsequent generations of perpetrators of war crimes and crimes
   against humanity suffer pathology that is in several respects similar to
   the transmission of trauma suffered by children of victims, if not more
   difficult to resolve. This paper aims to illustrate the consequences of
   the secrecy and distortion of reality in the families of perpetrators.
   Unacknowledged guilt and fear of social ostracism leads to social
   avoidance and shame. The (grand)children of perpetrators bear an extra
   burden due to their split loyalties. On the one hand they are
   unconsciously loyal to their parents, on the other hand they disapprove
   of their past and subscribe to diametrically opposite social norms.
   Seeking the truth and a desire to know the facts, a prerequisite for
   healing, is made difficult if not impossible by loyalty to the
   (grand)parents.},
