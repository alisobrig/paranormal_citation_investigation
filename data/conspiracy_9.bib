@article{ WOS:000562846400019,
Author = {Blessios, Athanassios G.},
Title = {The tragedies of Andreas Calvos},
Journal = {CAHIERS BALKANIQUES},
Year = {2020},
Volume = {47},
Pages = {303-318},
Abstract = {Andreas Calvos wrote three Italian tragedies (Le Danaidi, Teramene,
   Ippia) and a plan of tragedy (Abantida) around the second decade of the
   19th century. The tragedy Le Danaidi was the only one edited during the
   years 1818 and 1820. They utilize the ancient myth and history. Calvos
   develops the anti-tyranny subject area. Following the rules of
   classicism, he showed tyrants' characteristics, and the flume of
   conspiracy against them (Teramene, Ippia). The tragedy Le Danaidi
   follows the Greek tragedy both in structure and theme. The simplicity of
   the dramatic instruments culminates and the dramatic intensity is
   combined with the poetic power. Despite their partial inadequacies,
   these tragedies are characterised by dramatic quality and ideological
   power, which is concretised by the conflict of opposite powers right to
   the end.},
