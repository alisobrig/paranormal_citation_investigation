@article{ WOS:000660628000003,
Author = {Bausi, Francesco},
Title = {Love, anger, melancholy. The philosophical studies of the young
   Poliziano},
Journal = {LETTERE ITALIANE},
Year = {2020},
Volume = {72},
Number = {3},
Pages = {514-534},
Abstract = {The philosophical interests and studies cultivated by Poliziano in the
   second half of the 1470s, which concentrated on themes of moral
   philosophy (especially in regard to the risks and control of passionate
   excesses), are manifested both in the Latin translations of certain
   booklets and short treatises by Plutarch, Alexander of Aphrodisias and
   Epictetus, and in some original works (his De ira, his Latini, the
   letter to Lucrezia Tornabuoni of 18 December 1478, but also, to some
   extent, his Orfeo and Stanze). These interests and studies are traced
   back, not to abstract points of speculation (and much less to simple
   curiosity, occasional thoughts or intentions of ``escapism{''}), but to
   precise biographical experiences lived in those years by Poliziano
   himself and Lorenzo de' Medici, as well as to the difficult political
   situation in Florence in the aftermath of the Pazzi conspiracy.},
