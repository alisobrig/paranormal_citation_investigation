
@article{ WOS:000435006200002,
Author = {Wallhead, Celia M. and Leismer, Melissa},
Title = {Accommodating different conspiratorial views on 9/11 through ambiguity
   in <i>Bleeding Edge</i>},
Journal = {CRITIQUE-STUDIES IN CONTEMPORARY FICTION},
Year = {2018},
Volume = {59},
Number = {4},
Pages = {397-418},
Abstract = {Important figures such as Don DeLillo and Roger Rosenblatt claimed that
   9/11 saw the death of irony, but over a decade later, Thomas Pynchon
   uses strategies of ironic ambiguity in Bleeding Edge to accommodate an
   array of differing views on the attacks on the World Trade Center
   without seeming to opt for any one of them. Each one is shrouded in
   ambiguity, expressing only one side of the question. Pynchon's narrative
   strategies that allow it are, first, a narrator with a very peculiar
   voice, not omniscient, but with special powers of access; a protagonist
   whose profession and family life bring her into contact with contrasting
   figures; but, most of all, he has used all his old strategies of
   deliberate lack of information, double meanings, overloaded words,
   breaking the illusion of the ordinary, the reader's ignorance,
   ambivalent use of metaphor, allegory, and fantasy. This presentation of
   a complicated state of mind corresponds to William Empson's fourth type
   of ambiguity.},
