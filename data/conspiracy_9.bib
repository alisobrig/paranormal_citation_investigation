@article{ WOS:000442189100004,
Author = {Charette, Danielle},
Title = {CATILINARIAN CADENCES IN MACHIAVELLI'S <i>FLORENTINE HISTORIES</i>:
   CICERONIAN CONSENSUS AND CORRUPTED HUMANISM},
Journal = {HISTORY OF POLITICAL THOUGHT},
Year = {2018},
Volume = {39},
Number = {3},
Pages = {439-464},
Month = {FAL},
Abstract = {In debates over Machiavelli's uneasy relationship with classical
   republicanism, readers have overlooked the subversive uses to which
   Machiavelli puts Cicero and the Catiline conspiracy in his Florentine
   Histories. The speeches Machiavelli writes for Florence's nobili reveal
   leaders well-rehearsed in humanist language, even as they perpetrate
   crimes against the city. Machiavelli's critique of the elite's ability
   to thwart popular tumults is underscored by his allusions to Cicero
   especially Cicero's account of Catiline. These references offer
   Machiavelli an opportunity both to expose the Ciceronian style of
   promoting concord over conflict and to challenge Florentine
   historiographers, who had become oddly preoccupied with Catiline's
   Tuscan roots.},
