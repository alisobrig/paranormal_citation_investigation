@article{ WOS:000223996700005,
Author = {Stockwell, S},
Title = {Trade, empire, and the fiscal context of imperial business during
   decolonization},
Journal = {ECONOMIC HISTORY REVIEW},
Year = {2004},
Volume = {57},
Number = {1},
Pages = {142+},
Month = {FEB},
Abstract = {Recent studies have shown convincingly that no `neocolonialist'
   conspiracies were hatched to perpetuate British commercial dominance in
   the former colonies after independence, and that relations between
   individual firms and policy-makers were frequently troubled. In
   acknowledging the force of this general proposition, however, there is a
   risk of neglecting the still significant place of commercial
   considerations in state policy making. By relocating the relationship of
   trade and empire in the 1950s in an examination of a hitherto neglected
   dimension of British taxation policy, this article demonstrates that the
   Conservative government sought to assist British business with colonial
   interests at a time when these firms faced new uncertainties.},
