@article{ WOS:000609006500019,
Author = {Feuillet, Lucia},
Title = {<i>Plot devices in</i> Kentukis, <i>by Samanta Schweblin</i>},
Journal = {MITOLOGIAS HOY-REVISTA DE PENSAMIENTO CRITICA Y ESTUDIOS LITERARIOS
   LATINOAMERICANOS},
Year = {2020},
Volume = {22},
Pages = {317-335},
Month = {DEC},
Abstract = {Kentukis thematizes the issues of contemporary science fiction from an
   intimate enunciation which exhibits the sinister human bonds in a
   technological key. We propose an allegorical reading of the darkest
   aspects of the narrated private lives, which encrypt the fragmented and
   antagonistic nature of the social totality. The plot is the instrument
   that enables this analysis, since it works as a mediation between the
   individual text and the collective meaning, from the interpretative
   dimensions of the materialistic hermeneutics. Thus, the narrated
   micro-conspiracies say as much about the secret trait of reality as they
   do about fiction mechanisms.},
