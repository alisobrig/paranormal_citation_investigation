@article{ WOS:000436459000003,
Author = {Eldarov, Svetlozar},
Title = {THE REAL ``BULGARIAN CONNECTION{''}. BULGARIAN - VATICAN RELATIONS FROM
   JOHN XXIII TO JOHN PAUL II (1958 - 2005)},
Journal = {ISTORIYA-HISTORY},
Year = {2014},
Volume = {22},
Number = {4},
Pages = {378-386},
Abstract = {The relations between Bulgaria and the Vatican were shadowed in the
   collective consciousness by the so-called ``Bulgarian connection{''} in
   the assassination attempt against Pope John Paul II. During his visit to
   Bulgaria in the spring of 2002, the Pope said he did not believe in this
   conspiracy aimed at calumniation of the Bulgarian people and highlighted
   the deep historical ties between Bulgaria and the Holy See which dated
   back to the Slavic Apostles Cyril and Methodius in the Ninth century.
   This article deals with Bulgarian - Vatican relations at a time when
   heads of the Catholic Church were John XXIII, Paul VI and John Paul II.
   In the basis of these relations was immured the heritage of Cyril and
   Methodius as an integral part of the Bulgarian cultural and national
   identity on which many initiatives of mutual interest were developed.},
