@article{ WOS:000281496200004,
Author = {Holloway, Julian},
Title = {Legend-tripping in spooky spaces: ghost tourism and infrastructures of
   enchantment},
Journal = {ENVIRONMENT AND PLANNING D-SOCIETY \& SPACE},
Year = {2010},
Volume = {28},
Number = {4},
Pages = {618-637},
Month = {AUG},
Abstract = {This paper investigates the increasingly popular practice of ghost
   tourism comprising urban ghost tours and organised paranormal
   investigations. Set in the context of modern forms of enchantment,
   wherein audiences engage in a knowing and reflexive sense of `delight
   without delusion', the paper explores the various infrastructures,
   (discursive, affective, and material) that engender a sense of
   supernatural possibility. I argue that practices of legend-telling,
   legend-tripping, ostension, and play produce affective assemblages of
   supposition and wonder that momentarily transform space into something
   charged with the strange and anomalous. This infrastructure relics on
   the engineering of dispositions that are both shaped on and brought to
