@article{ WOS:A1992JJ91200003,
Author = {ANDERSON, RI},
Title = {SEARCH FOR SHARADA - REPORT OF A CASE AND ITS INVESTIGATION - COMMENTARY},
Journal = {JOURNAL OF THE AMERICAN SOCIETY FOR PSYCHICAL RESEARCH},
Year = {1992},
Volume = {86},
Number = {3},
Pages = {249-256},
Month = {JUL},
Abstract = {Comparing thc reports of Akolkar and Stevenson on the Sharada case of
   responsive xenoglossy is instructive for a number of reasons. Besides
   confirming the main facts in the case, the agreements between the two
   reports show how easily the case lends itself to the reincarnation
   explanation. This does not mean that no other explanation will do, but
   it does indicate just how difficult it is to dismiss the case as due to
   fraud. cryptomnesia, or investigative incompetence. All are still
   possible interpretations, especially that of cryptomnesia, but the case
   as it now stands requires something more than the mouthing of airy
   possibilities to escape the paranormal interpretation favored by both
   investigators.},
