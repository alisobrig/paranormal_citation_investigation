@article{ WOS:A1994QH41500009,
Author = {ZIKMUND, V},
Title = {PSYCHOTRONICS - AN UNSUCCESSFUL ATTEMPT OF PARAPSYCHOLOGY AND
   PARAMEDICINE TO BECOME SCIENCE},
Journal = {STUDIA PSYCHOLOGICA},
Year = {1994},
Volume = {36},
Number = {3},
Pages = {223-226},
Abstract = {Psychotronics was founded in Czechoslovakia in 1967 as an
   interdisciplinary field dealing with the same paranormal phenomena as
   did parapsychology.  In psychotronics the scientific, materialistic
   approach was stressed and in the last 20 years several international and
   national conferences were organized, taking place mostly in socialist
   and postsocialist countries.  Unfortunately, no methods and results of
   psychotronic investigations were published in technical journals and/or
   publications accessible to scientific public.  Psychotronics remained
   unknown in the scientific world and no distinct difference was found
   between the approaches to the paranormal in psychotronics and in
   parapsychology.  At present, it is mostly exploited as a
   pseudoscientific argument for claims of various paranormal effects and
   influences in folk medicine and psychology.},
