@article{ WOS:000220737100003,
Author = {Fukushima, K},
Title = {Conspiracy of form and function for optimization of language change},
Journal = {JOURNAL OF EAST ASIAN LINGUISTICS},
Year = {2004},
Volume = {13},
Number = {2},
Pages = {181-196},
Month = {APR},
Abstract = {Formal and functional approaches to language have been at odds with each
   other for some time. however, this state of affairs is not constructive
   or productive. In this paper I will show that an ongoing morphological
   change in Japanese, ra-nuki kotoba `ra-deletion language', initiated by
   functional motivation like economy (communicative efficiency), is guided
   and shaped most effectively by interacting formal (syntactic/semantic)
   properties of related constructions. Through the exposition in this
   paper I would like to demonstrate that there is nothing incompatible
   between the orientations of formalism and functionalism The former
   provides descriptions and explanations for structural possibilities (or
   delimitation) of language. From these possibilities, certain forms are
   chosen (or preferred) over others for various purposes (communication,
   innovation, etc.) according to the principles and generalizations of the
   latter.},
