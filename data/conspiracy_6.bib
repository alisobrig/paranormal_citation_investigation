@article{ WOS:000409111700035,
Author = {Borza, Liana Rada and Gavrilovici, Cristina},
Title = {ETHICAL INTERPRETATIONS OF PATIENT AUTONOMY IN CHRONIC CARE WITH SPECIAL
   REFERENCE TO CANCER CARE},
Journal = {MEDICAL-SURGICAL JOURNAL-REVISTA MEDICO-CHIRURGICALA},
Year = {2015},
Volume = {119},
Number = {3},
Pages = {835-842},
Abstract = {The present review analyzes the ethical interpretations of patient
   autonomy in chronic care with special reference to the advanced cancer
   patient and to the transition from curative to palliative. Thus, in the
   context of chronic care, studies indicate that a positive
   conceptualization of autonomy, which is compatible with the assumption
   that people are interdependent, is preferred over a negative
   conceptualization that presents autonomy as non-interference. Moreover,
   it has been shown that the concept of ``Socratic autonomy{''}, which
   promotes an ethics of care and of interdependence, represents the
   standard for assessing autonomy in chronic care. With regard to cancer
   care, several studies illustrate that most patients decide to delegate
   the decisional responsibility completely to their physician and such
   evidence support the interpretation of autonomy as critical reflection.
   This paper also reviews the ethical challenges that may derive from
   delivering bad news to advanced cancer patients, pointing out that
   disclosure of a cancer diagnosis or of a poor prognosis can facilitate a
   so-called conspiracy of silence.},
