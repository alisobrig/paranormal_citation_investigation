@article{ WOS:000640892100001,
Author = {Matschke, Xenia and Rieger, Marc Oliver},
Title = {Kisses, Handshakes, COVID-19 - Will the Pandemic Change Us Forever?},
Journal = {REVIEW OF BEHAVIORAL ECONOMICS},
Year = {2021},
Volume = {8},
Number = {1},
Pages = {25-46},
Abstract = {We study the changes in greeting behavior caused by the COVID-19
   pandemic. We find that after the use of handshakes and social kissing as
   forms of greeting decreased drastically, they became common again
   already a few months after the outbreak. Nevertheless, a consistently
   large proportion of respondents (around 57\%) plan to change their
   greeting behavior permanently, either because they simply became used to
   it or because they want to avoid the danger of contracting and
   transmitting infectious diseases. As can be seen from the results of the
   first survey wave in April, 2020, the belief in conspiracy theories
   about COVID-19 did not influence the greeting behavior of individuals,
   but it proved to have a strong impact in the second survey wave in
   September, 2020.},
