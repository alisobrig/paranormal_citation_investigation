@article{ WOS:000251718600005,
Author = {Fassin, Didier},
Title = {Between national desire and conspiracy theory. Medicine ideologies in
   South Africa},
Journal = {SCIENCES SOCIALES ET SANTE},
Year = {2007},
Volume = {25},
Number = {4},
Pages = {93-114},
Month = {DEC},
Abstract = {Drugs have recently become an object of research for social sciences.
   Whereas they have mainly been examined in terms of their representations
   and uses, production procedures and evaluation processes, the present
   paper approaches a dimension little studied: the ideological investment
   it provokes. Based on a study of the controversies around antiretroviral
   drugs in South Africa, it deals with two particular ideological figures
   of the dispute against Western biomedical hegemony: therapeutic
   nationalism, valorising the production of locally discovered treatments;
   iatrogenic paranoia, denouncing the dangers of imported medicines.
   Beyond the South African case, a reflection is proposed about the
   historical and political issues at stake through these ideologies in the
   Third World.},
