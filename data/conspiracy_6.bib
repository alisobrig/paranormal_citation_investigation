@article{ WOS:000416483100008,
Author = {Labuschagne, Pieter},
Title = {The political and cultural symbolism of trees as representational and
   transcendence objects},
Journal = {SOUTH AFRICAN JOURNAL OF ART HISTORY},
Year = {2016},
Volume = {31},
Number = {1},
Pages = {113-125},
Month = {JAN},
Abstract = {Throughout history the role of trees to occupy and dominate space have
   been appreciated, acknowledged an acted on by most societies. In this
   article a botanical but also a historical and cultural perspective on
   the symbolism of trees as transcending and representational devices were
   provided. The aim of the article was to look at the symbolic and
   historical dimensions of trees within a wider context. In order to
   conduct the study, reference was made to studies that have been done
   internationally in this regard. However, the central focus was on trees
   of historical significance in South Africa. The selection of trees
   transcends more than just botanical objects but also functions
   historically and culturally on a symbolic level to transcend and
   represent deeper historical values. The trees that were selected for
   this purpose were the Ultimatum Tree in KwaZulu-Natal, the Conspiracy
   Tree in the Free State and the Milkwood Tree in the Eastern Cape. In
   each of the examples the investigation was done with reference to the
   trees' role and value as transitional and transcending objects.},
