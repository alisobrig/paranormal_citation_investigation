@article{ WOS:000499722500013,
Author = {Khlopov, Maxim Yu.},
Title = {Removing the conspiracy of BSM physics and BSM cosmology},
Journal = {INTERNATIONAL JOURNAL OF MODERN PHYSICS D},
Year = {2019},
Volume = {28},
Number = {13, SI},
Month = {OCT},
Abstract = {The standard model (SM) of elementary particles finds no contradictions
   in the experimental data, but appeals to extensions for solutions of its
   internal problems and physical basis of the modern cosmology. The latter
   is based on inflationary models with baryosynthesis and dark
   matter/energy that involves Physics beyond the standard model (BSM) of
   elementary particles. However, studies of the BSM physical basis of the
   modern cosmology inevitably reveals additional particle model-dependent
   cosmological consequences that go beyond the modern standard
   cosmological model. The mutual relationship of the BSM particle physics
   basis of the modern cosmology and the nontrivial features of the
   corresponding cosmological scenario are the subject of this paper.},
