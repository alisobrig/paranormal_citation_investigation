@article{ WOS:000421463700016,
Author = {Facio Lince, Victoria Diaz and Ruiz Osorio, Mario Alberto and Florez
   Ruiz, Carla and Urrea Cosme, Yenny and Cordoba Sanchez, Veronica and
   Camilo Arbelaez, Cristian and Rodriguez Zabala, David},
Title = {The Dying Process: Destination and Significance of Imminent Death
   Diagnosis},
Journal = {REVISTA VIRTUAL UNIVERSIDAD CATOLICA DEL NORTE},
Year = {2013},
Volume = {39},
Pages = {195-211},
Month = {MAY-AUG},
Abstract = {This article presents a reflection derived from the qualitative research
   called ``Meaning of the Diagnosis of Imminent Death for the Patient with
   Cancer and his/her Family, and its Effects on the Evolution of the Dying
   Process{''}, in which a hermeneutical phenomenological approach was
   used. The reflection is focused on one of the main findings concerning
   the management and course of the information once it is revealed to the
   family. Our results show that the diagnosis of imminent death shakes the
   family structure, generating attitudes such as defensive reactions, a
   conspiracy of silence, overprotection and excessive compassion. We
   discuss these reactions and give emphasize to the paradoxes and
   contradictions implied in the actions that are, supposedly, adopted as
   mechanisms to protect from suffering to the patient and the family.},
