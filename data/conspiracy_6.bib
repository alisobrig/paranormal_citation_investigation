@article{ WOS:000086315000005,
Author = {Chen, JJR and Liu, Y},
Title = {An ID-based digital multisignatures scheme with time stamp technique},
Journal = {COMPUTER SYSTEMS SCIENCE AND ENGINEERING},
Year = {2000},
Volume = {15},
Number = {2},
Pages = {105-109},
Month = {MAR},
Abstract = {The RSA theory is designed for a single signature with verification load
   O(1).If the RSA theory is applied to digital multisignature, both
   transmission volume and signatures Verification create a heavy
   calculation load O(n). In this paper, we propose a digital
   multisignature scheme to prevent possible attacks, i.e., impersonate
   attack, interpolation, replay attack, cryptanalysis and conspiracy
   attack. Our results indicate that the complexities of both the
   factorization of the RSA theory and the discrete logarithm of Shamir
   Id-based Cryptosystem are associated with Denning and Sacco time stamp
   scheme in digital multisignature. Our digital mulitsignature scheme
   allows a sender to determine the priority of mailing documents and
   enjoys O(1) in transmission volume and load in the processes of
   encryption, decryption, and signatures verification.},
