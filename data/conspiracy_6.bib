@article{ WOS:000307162700002,
Author = {Berman, Nathaniel},
Title = {`The Sacred Conspiracy': Religion, Nationalism, and the Crisis of
   Internationalism},
Journal = {LEIDEN JOURNAL OF INTERNATIONAL LAW},
