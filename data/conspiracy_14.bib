@article{ WOS:000302225000005,
Author = {Beer, Linda and Fagan, Jennifer L. and Garland, Pamela and Valverde,
   Eduardo E. and Bolden, Barbara and Brady, Kathleen A. and Courogen,
   Maria and Hillman, Daniel and Neaigus, Alan and Bertolli, Jeanne and
   Never Care Project},
Title = {Medication-Related Barriers to Entering HIV Care},
Journal = {AIDS PATIENT CARE AND STDS},
Year = {2012},
Volume = {26},
Number = {4},
Pages = {214-221},
Month = {APR},
Abstract = {Early entry to HIV care and receipt of antiretroviral therapy improve
   the health of the individual and decrease the risk of transmission in
   the community. To increase the limited information on prospective
   decisions to enter care and how these decisions relate to beliefs about
   HIV medications, we analyzed interview data from the Never in Care
   Project, a multisite project conducted in Indiana, New Jersey, New York
   City, Philadelphia, and Washington State. From March 2008 through August
   2010, we completed structured interviews with 134 persons with no
   evidence of HIV care entry, 48 of whom also completed qualitative
   interviews. Many respondents believed that HIV care entails the passive
   receipt of medications that may be harmful or unnecessary, resulting in
   reluctance to enter care. Respondents voiced concerns about prescription
   practices and preserving future treatment options, mistrust of
   medications and medical care providers, and ambivalence about the
   life-preserving properties of medications in light of an assumed
   negative impact on quality of life. Our results support the provision of
   information on other benefits of care (beyond medications), elicitation
   of concerns about medications, and assessment of psychosocial barriers
   to entering care. These tasks should begin at the time a positive test
   result is delivered and continue throughout the linkage-to-care process;
   for persons unwilling to enter care immediately, support should be
   provided in nonmedical settings.},
