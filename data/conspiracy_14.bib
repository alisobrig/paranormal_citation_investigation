@article{ WOS:000407617700004,
Author = {Rykov, Yuri G. and Meylakhs, Peter A. and Sinyavskaya, Yadviga E.},
Title = {Network Structure of an AIDS-Denialist Online Community: Identifying
   Core Members and the Risk Group},
Journal = {AMERICAN BEHAVIORAL SCIENTIST},
