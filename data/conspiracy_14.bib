@article{ WOS:000359529200003,
Author = {Pearce, Katy E.},
Title = {Democratizing kompromat: the affordances of social media for
   state-sponsored harassment},
Journal = {INFORMATION COMMUNICATION \& SOCIETY},
Year = {2015},
Volume = {18},
Number = {10},
Pages = {1158-1174},
Month = {OCT 3},
Abstract = {This study examines how a traditional tool of authoritarian social
   control - harassment of the opposition - has evolved with the need for
   more subtle harassment in the twenty-first-century transnational
   activism and social media era. Social media affords cheap and easy
   opportunities for authoritarian regimes, in this case, Azerbaijan, to
   subtly harass opposition to a large domestic audience, while eschewing
   direct attribution of the harassment. In this way, despite the
   challenges that information and communication technologies present
   authoritarian regimes, they also provide opportunities to increase
   control.},
