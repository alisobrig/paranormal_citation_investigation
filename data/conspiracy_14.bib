@article{ WOS:000319075900004,
Author = {Grossman, Cynthia I. and Purcell, David W. and Rotheram-Borus, Mary Jane
   and Veniegas, Rosemary},
Title = {Opportunities for HIV Combination Prevention to Reduce Racial and Ethnic
   Health Disparities},
Journal = {AMERICAN PSYCHOLOGIST},
Year = {2013},
