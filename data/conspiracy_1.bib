@article{ WOS:000450327400008,
Author = {van Prooijen, Jan-Willem and van Vugt, Mark},
Title = {Conspiracy Theories: Evolved Functions and Psychological Mechanisms},
Journal = {PERSPECTIVES ON PSYCHOLOGICAL SCIENCE},
