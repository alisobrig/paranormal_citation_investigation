@article{ WOS:000784068500001,
Author = {Halford, Sarah J.},
Title = {Conspiracy Movements: A Definitional Introduction and Theoretical
   Exploration of Organized Challenges to Epistemic Authority},
Journal = {SOCIOLOGICAL QUARTERLY},
