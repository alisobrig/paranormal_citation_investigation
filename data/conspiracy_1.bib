@article{ WOS:000507968800002,
Author = {Stojanov, Jelka and Stanisavljavic, Sara and Tatic, Visnja and Pantic,
   Aleksa},
Title = {CONSPIRACY THINKING INVENTORY (CTI) - CONSTRUCTION AND VALIDATION STUDY},
Journal = {PRIMENJENA PSIHOLOGIJA},
