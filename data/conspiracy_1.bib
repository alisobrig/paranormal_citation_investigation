   Olivier},
Title = {`Who wants to silence us'? Perceived discrimination of conspiracy theory
   believers increases `conspiracy theorist' identification when it comes
   from powerholders - But not from the general public},
Journal = {BRITISH JOURNAL OF SOCIAL PSYCHOLOGY},
