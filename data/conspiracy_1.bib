@article{ WOS:000849038700001,
Author = {Frostad Haakonsen, Jon Magnus and Furnham, Adrian},
Title = {COVID-19 Vaccination: Conspiracy Theories, Demography, Ideology, and
   Personality Disorders},
Journal = {HEALTH PSYCHOLOGY},
Year = {2023},
