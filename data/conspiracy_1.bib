@article{ WOS:000417415300009,
Author = {Galliford, Natasha and Furnham, Adrian},
Title = {Individual difference factors and beliefs in medical and political
   conspiracy theories},
Journal = {SCANDINAVIAN JOURNAL OF PSYCHOLOGY},
Year = {2017},
Volume = {58},
Number = {5},
Pages = {422-428},
Month = {OCT},
Abstract = {This study examined the relationship between a series of individual
   difference measures and belief in political and medical conspiracy
   theories. Participants (N = 323) rated 20 conspiracy theories (10
   medical, 10 political) and completed a set of questionnaires. Belief in
   political conspiracies was strongly positively correlated with belief in
   medical conspiracies. Belief in both conspiracy types was correlated
   with low self-esteem, low Conscientiousness, more right-wing political
   views, younger age, and greater belief in the benefits of Alternative
   Medicine. It was also correlated with religiousness and gender. Low
   Emotional Stability and Agreeableness were also correlated with belief
   in political conspiracies, and higher education level was correlated
   with belief in medical conspiracies. The findings generally demonstrated
   support for a monological belief system. Implications and limitations
   are discussed.},
