@article{ WOS:000432815800005,
Author = {Neville-Shepard, Ryan},
Title = {Paranoid Style and Subtextual Form in Modern Conspiracy Rhetoric},
Journal = {SOUTHERN COMMUNICATION JOURNAL},
Year = {2018},
Volume = {83},
Number = {2},
Pages = {119-132},
Abstract = {This essay argues that the traditional approach to conspiracy rhetoric,
   grounded in Richard Hofstadter's concept of the paranoid style, is
   limited in understanding as to how such fringe discourse is produced to
   resonate with mainstream audiences. Using the 9/11 conspiracy theory and
   the work of 9/11 scholar David Ray Griffin as a case study, I propose
   the existence of a subtextual form in conspiracy rhetoric that is
   anti-establishment, critiques dominant political myths, and offers a
   broad-based political ideology. After exploring how this approach
   relates to the 9/11 conspiracy theory, the essay considers the
   implications that this analysis has for understanding modern conspiracy
   discourse.},
