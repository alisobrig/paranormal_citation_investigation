@article{ WOS:001001130100002,
Author = {Frisone, Fabio},
Title = {PSYCHOPATHOLOGY OF DELUSION AN ATTEMPT TO CLARIFY THE OVERLAP BETWEEN
   CONSPIRACY THEORIES AND DELUSIONS},
Journal = {RETI SAPERI LINGUAGGI-ITALIAN JOURNAL OF COGNITIVE SCIENCES},
Year = {2022},
Volume = {11},
Number = {1},
Pages = {65-85},
Month = {JAN-JUN},
Abstract = {As Bortolotti et al. (2021) highlighted, the understanding of conspiracy
   theories and delusions has not been entirely clear. The aim of this
   study is to examine the char-acteristics of the different types of
   delusions seen in mental disorders. After the psychopathology of
   delusion is clarified, the characteristics of conspiracy theories are
   examined. This study takes up some ideas that emerged from the
   combination that Bortolotti et al. (2021) proposed between conspiracy
   theories and delusions. The result of the research makes it clear that
   conspiracy theories and delusions follow a similar path, at least up to
   a point, for this reason these phenomena should not always be
   interpreted as not being comparable.},
