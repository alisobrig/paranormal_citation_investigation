@article{ WOS:000905494400007,
Author = {Kyriakidis, Savvas},
Title = {The Use of Oaths in the Conspiracies and Revolts against Andronikos II
   Palaiologos (1282-1328)},
Journal = {JAHRBUCH DER OESTERREICHISCHEN BYZANTINISTIK},
Year = {2021},
Volume = {71},
Pages = {269-284},
Abstract = {The purpose of this paper is to examine the oaths which were exchanged
   between leaders and supporters of conspiracies and rebellions against
   Andronikos II Palaiologos (1282-1328). It investigates the content of
   these oaths and the rituals which were used in order to increase their
   binding power. It explores the effectiveness of oaths in solidifying
   alliances and trust between conspirators. It also examines the oaths of
   loyalty Andronikos II demanded from imperial subjects in response to
   conspiracies against him and discusses how conspirators justified
   breaking oaths of loyalty to the emperor. The analysis of these oaths
   contributes to a better understanding of the organization of
   conspiracies in the later Byzantine period.},
