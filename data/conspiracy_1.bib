@article{ WOS:000869165200001,
Author = {Ramonaite, Aine},
Title = {Does communist nostalgia lead to COVID-19 conspiracy beliefs?},
Journal = {EUROPEAN SOCIETIES},
Year = {2023},
Volume = {25},
Number = {3},
Pages = {489-508},
Month = {MAY 27},
Abstract = {Why are people in Central and Eastern Europe more hesitant towards
   COVID-19 vaccination and more prone to believe in COVID-19 related
   conspiracy theories than other Europeans? The article claims that the
   spread of COVID-19 conspiracy beliefs in the post-communist region might
   be fostered by communist nostalgia. Drawing on the survey data from
   Lithuania, I show that communist nostalgia is one of the best predictors
   of COVID-19 conspiracy beliefs, controlling for other related factors
   such as populist attitudes, trust in political institutions, confidence
   in media and scientists and pro-Western attitudes. The paper claims that
   communist nostalgia in Central and Eastern Europe is conducive to
   conspiracy beliefs in a similar vein as nostalgic narratives employed by
   populist radical right in Western countries.},
