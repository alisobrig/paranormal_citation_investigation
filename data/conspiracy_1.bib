@article{ WOS:000376897300003,
Author = {van Tongeren, Carlos},
Title = {Confronting Conspiracies in Manuel Vazquez Montalban's Late Carvalho
   Novels},
Journal = {NEOPHILOLOGUS},
Year = {2016},
Volume = {100},
Number = {3},
Pages = {373-387},
Month = {JUL},
Abstract = {This essay examines the conspiratorial worldviews of three of Manuel
   Vazquez Montalban's novels on detective Pepe Carvalho. It claims that
   Vazquez Montalban's conspiracy narratives, due to their preoccupation
   with the (in)adequacy of names and actions within a conspiratorial
   totality, complicate both their own gesture of social criticism and the
   possibilities of rebellion by literary characters. Moreover, the article
   shows that conspiracies are located at the upper-end of the social
   spectrum in these novels. Unlike other contemporary writers of detective
   fiction in Spanish, then, Vazquez Montalban stops short of imagining
   society as a productive battlefield between opposing complots.},
