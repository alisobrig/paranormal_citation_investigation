@article{ WOS:000373473100002,
Author = {Matheson, Calum},
Title = {``What does Obama want of me?{''} Anxiety and Jade Helm 15},
Journal = {QUARTERLY JOURNAL OF SPEECH},
Year = {2016},
Volume = {102},
Number = {2},
Pages = {133-149},
Month = {APR 2},
Abstract = {This article reclaims the concept of anxiety for rhetoric and
   demonstrates the usefulness of doing so by examining reactions to the
   Jade Helm 15 military training exercise. Following Lacan's recently
   translated seminar on Anxiety, the essay argues on behalf of a
   conceptualization of anxiety as a result of affective investments in a
   network of discourse rather than an individual feeling. This
   rearticulation of anxiety has implications for both rhetorical theory
   and criticism as well as practical responses to right-wing conspiracy
   theories such as those surrounding Jade Helm.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
