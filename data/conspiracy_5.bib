@article{ WOS:000263049000008,
Author = {Budnitskii, Oleg},
Title = {The Jews and revolution: Russian perspectives, 1881-1918},
Journal = {EAST EUROPEAN JEWISH AFFAIRS},
Year = {2008},
Volume = {38},
Number = {3},
Pages = {321-334},
Abstract = {Contemporaries and historians of the Russian revolutions have often made
   a great deal of the Jewish role in the events of 1917. In late 1917, for
   instance, it was commonplace to assert that the Bolsheviks were simply
   part of a Jewish conspiracy. This paper takes a look at the various
   Russian perspectives on the revolution and its Jewish aspects, focusing
   in particular on the views of leading intellectuals, writers and
   politicians, such as S. M. Dubnov, Maxim Gorkii and Aleksandr Blok. It
   reveals that attitudes were not always straightforward, even amongst the
   liberal elements of the intelligentsia.},
