@article{ WOS:000758760000001,
Author = {Rothmund, Tobias and Farkhari, Fahima and Ziemer, Carolin-Theresa and
   Azevedo, Flavio},
Title = {Psychological underpinnings of pandemic denial-patterns of disagreement
   with scientific experts in the German public during the COVID-19
   pandemic},
Journal = {PUBLIC UNDERSTANDING OF SCIENCE},
Year = {2022},
Volume = {31},
Number = {4},
Pages = {437-457},
Month = {MAY},
Abstract = {We investigated pandemic denial in the general public in Germany after
   the first wave of COVID-19 in May 2020. Using latent class analysis, we
   compared patterns of disagreement with claims about (a) the origin,
   spread, or infectiousness of the SARS-CoV-2 virus and (b) the personal
   risk from COVID-19 between scientific laypersons (N = 1,575) and
   scientific experts (N = 128). Two groups in the general public differed
   distinctively from expert evaluations. The Dismissive (8\%) are
   characterized by low-risk assessment, low compliance with containment
   measures, and mistrust in politicians. The Doubtful (19\%) are
   characterized by low cognitive reflection, high uncertainty in the
   distinction between true and false claims, and high social media intake.
   Our research indicates that pandemic denial cannot be linked to a single
   and distinct pattern of psychological dispositions but involves
   different subgroups within the general population that share high
   COVID-19 conspiracy beliefs and low beliefs in epistemic complexity.},
