@article{ WOS:000211328200006,
Author = {Thorne, Kym and Johnston, Judith},
Title = {Benign Partners or Partners in Crime?},
Journal = {PUBLIC INTEGRITY},
Year = {2012},
Volume = {14},
Number = {2},
Pages = {203-210},
Abstract = {This paper responds to Kee and Forrer's critique of Kouzmin, Johnston,
   and Thorne's (2011) examination of political and financial conspiracies
   involving elites and other self-serving interests connected to
   Private-Public Partnerships (PPPs). In particular, it assesses Kee and
   Forrer's de-contextualized, ahistorical neoliberal proposals and
   organization-level list of critical administration items required to
   eliminate any possible anti-democratic manipulations within
   ``individualized{''} and ``disconnected{''} PPPs. This paper asserts the
   systemic vulnerability of PPPs to State Crimes against Democracy (SCADS)
   and Economic State Crimes against Democracy (E-SCADS). It concludes that
   the stakeholders must continually interrogate these partnerships to
   determine whether they are symbiotic, parasitical, destructive or
   criminogenic in regards to the public interest and democratic
   citizenship.},
