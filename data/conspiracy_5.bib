@article{ WOS:000582631100004,
Author = {Joslin, Isaac},
Title = {Algerian crises and baroquism in Le oath des barbares by Boualem Sansal},
Journal = {FRENCH STUDIES IN SOUTHERN AFRICA},
Year = {2020},
Number = {50},
Pages = {60-82},
Month = {OCT},
Abstract = {This paper theorises Boualem Sansal's detective novel, Le serment des
   barbares, passes through the lens of a baroque aesthetic, which
   highlights the complexities of historical constructs and violent
   conflicts in particularly insightful ways when applied to the
   sociocultural context of postcolonial Algeria. In concert with Gregg
   Lambert's formulations of The Return of the Baroque in Modern Culture
   and Paul Silverstein's analysis of conspiracy theories in postcolonial
   Algeria, this article demonstrates the ways in which Sansal's stylistic
   and thematic choices are emblematic of a baroque practice that
   overwhelms the reader with a chaotic representation of Algerian society
   in the turmoil of the political crisis of the 1990's. Through a close
   reading of the investigation of Sansal's protagonist, inspector Larbi,
   the melancholy and trauma of postcolonial society are carefully
   elaborated in the context of transnational politics and the control of
   information that is a hallmark characteristic of repressive regimes.
   Finally, through an analysis of the pervasive morbidity in Sansal's
   novel, the cadavers of victims are understood as allegories of Algerian
   conflicts, revealing the physical and psychological suffering of a
   citizenry faced with the ever-present realities of death and societal
   decay.},
