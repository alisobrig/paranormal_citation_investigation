@incollection{ WOS:000579138600004,
Author = {Leicester, Jonathan},
Book-Author = {Leicester, J},
Title = {Some Strange Shared Beliefs},
Booktitle = {WHAT BELIEFS ARE MADE FROM},
Year = {2016},
Pages = {5-13},
Abstract = {This chapter describes and comments on mistaken beliefs that are or have
   been orthodox or at least common in whole communities. So-called
   culture-bound psychoses are illustrated by shen-k'uei in Taiwanese
   culture and malgri among natives of Mornington Island. So-called
   epidemic hysteria is illustrated by an outbreak of witchcraft in
   Christian Europe, the epidemic of shell shock among allied soldiers in
   the First World War, and the epidemic of RSI in Australia from 1980 to
   1986. Brief notes on myths, legends, rumours, and conspiracy theories
   complete the chapter.},
