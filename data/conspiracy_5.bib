@article{ WOS:000881502200002,
Author = {Sazonov, Vladimir and Ploom, Illimar and Veebel, Viljar},
Title = {The Kremlin's Information Influence Campaigns in Estonia and Estonian
   Response in the Context of Russian-Western Relations},
