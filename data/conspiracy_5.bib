@article{ WOS:000684583500020,
Author = {Ilyin, V, P. and Shkerin, V. A.},
Title = {The Interregnum and the Saint Petersburg Revolt of the Decembrists as
   Reflected in the Post-Soviet Historiography},
Journal = {VESTNIK SANKT-PETERBURGSKOGO UNIVERSITETA-ISTORIYA},
