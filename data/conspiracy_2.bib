@article{ WOS:A1997WX44900004,
Author = {Smith, JC},
Title = {More on proving conspiracy},
Journal = {CRIMINAL LAW REVIEW},
Year = {1997},
Pages = {333-338},
Month = {MAY},
Abstract = {In Jones, Williams and Barham (1996) the court acted on the basis that
   any special principle governing the admissibility of hearsay evidence in
   a conspiracy case is equally applicable to a case of parties parries to
   a joint enterprise. It is submitted that this, though contrary to the
   observations in Gray and others (1994), is right in principle. The basis
   for the admission of the hearsay is the existence of independent prima
   facie evidence of the conspiracy or joint enterprise and it is for the
   judge alone to decide whether there is such evidence. If there is, the
   jury must be directed to consider ail the evidence but warned against
   convicting on the basis of the hearsay evidence alone.},
