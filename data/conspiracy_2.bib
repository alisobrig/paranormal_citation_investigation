@article{ WOS:000656313900001,
Author = {Dowhower, Daniel P. and Harvey, S. Marie and Oakley, Lisa P.},
Title = {Experiences of discrimination and endorsement of HIV/AIDS conspiracy
   beliefs: exploring difference among a sample of Latino, Black, and White
   young adults},
Journal = {ETHNICITY \& HEALTH},
