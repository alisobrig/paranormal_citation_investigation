@article{ WOS:000801978400001,
Author = {Brugger, Niels},
Title = {Tracing a historical development of conspiracy theory networks on the
   web: The hyperlink network of vaccine hesitancy on the Danish web
   2006-2015},
Journal = {CONVERGENCE-THE INTERNATIONAL JOURNAL OF RESEARCH INTO NEW MEDIA
   TECHNOLOGIES},
