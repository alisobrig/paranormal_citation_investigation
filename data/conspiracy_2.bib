@article{ WOS:000664690400001,
Author = {Cordonier, Laurent and Cafiero, Florian and Bronner, Gerald},
Title = {Why are conspiracy theories more successful in some countries than in
   others? An exploratory study on Internet users from 22 Western and
   non-Western countries},
Journal = {SOCIAL SCIENCE INFORMATION SUR LES SCIENCES SOCIALES},
Year = {2021},
Volume = {60},
Number = {3, SI},
Pages = {436-456},
Month = {SEP},
Abstract = {This exploratory study aims at identifying macro-social factors
   associated with the international variance of belief in conspiracy
   theories. We computed a Conspiracy Index for 22 Western and non-Western
   countries based on the results of an online survey on conspiratorial
   beliefs. Stepwise regression analyses show that more than 70\% of the
   international variance of this Conspiracy Index is explained by the
   following three national variables: the level of democracy, the
   unemployment rate, and the perceived level of public sector corruption.
   Conspiracy theories thus appear to be more commonly endorsed in
   countries where people cannot take an active part in the political life
   of their country (low level of democracy), where they may feel socially
   threatened (high unemployment rate), and where institutions and
   authorities are perceived as untrustworthy (high perception of public
   sector corruption).},
