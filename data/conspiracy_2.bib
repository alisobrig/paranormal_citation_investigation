@article{ WOS:000422726900004,
Author = {Choe, Key-Sook},
Title = {Media Cultural Politics and the Politics of Sympathy-<i>Queen
   Seondeok</i>: Dramatized Conspiracy ``Theory as ``Power-Narrative{''}},
Journal = {REVIEW OF KOREAN STUDIES},
