@article{ WOS:000215513100008,
Author = {Nicolas, Loic},
Title = {The Evidence of Conspiracy: a Challenge to Argumentation. Doubting
   everything, so as not to doubt at all},
Journal = {ARGUMENTATION ET ANALYSE DU DISCOURS},
Year = {2014},
Volume = {13},
Abstract = {This paper deals with the place, status and ambiguities of doubt in
   conspiracy theories. Its aim is to question, with Chaim Perelman, the
   argumentative properties and topical basis of these theories through the
   three terms: evidence, doubt, and critique. In conspiratorial
   perspectives, doubt invades the social sphere: the world is only an
   appearance, lies are everywhere. However, inside the discourse itself,
