@article{ WOS:000449178600006,
Author = {Hart, Joshua and Graether, Molly},
Title = {Something's Going on Here Psychological Predictors of Belief in
   Conspiracy Theories},
Journal = {JOURNAL OF INDIVIDUAL DIFFERENCES},
Year = {2018},
Volume = {39},
Number = {4},
Pages = {229-237},
Abstract = {Research on individual-difference factors predicting belief in
   conspiracy theories has proceeded along several independent lines that
   converge on a profile of conspiracy believers as individuals who are
   relatively untrusting, ideologically eccentric, concerned about personal
   safety, and prone to perceiving agency in actions and profundity in
   bullshit. The present research represents the first attempt at an
   integrative approach to testing the independent contributions of these
   diverse factors to conspiratorial thinking. Two studies (N = 1,253)
   found that schizotypy, dangerous-world beliefs, and bullshit receptivity
   independently and additively predict endorsement of generic (i.e.,
   nonpartisan) conspiracy beliefs. Results suggest that ``hyperactive{''}
   agency detection and political orientation (and related variables) might
   also play a role. The studies found no effects of situational threats
   (mortality salience or a sense of powerlessness) - though it remains to
   be seen whether real-world instantiations of situational threats might
   move some people to seek refuge in conspiratorial ideation.},
