@article{ WOS:000393493600011,
Author = {Beringer, Alex},
Title = {Humbug History The Politics of Puffery in <i>Tom Sawyer's Conspiracy</i>},
Journal = {MARK TWAIN ANNUAL},
Year = {2016},
Volume = {14},
Pages = {114-126},
Abstract = {This article reveals how Mark Twain's unfinished novel Tom Sawyer's
   Conspiracy offers a searing critique of how late nineteenth-century
   American politics were dangerously intertwined with the logic of popular
   entertainment. Critics have generally counted this fragment among pieces
   like Tom Sawyer, Detective or ``Huck and Tom among the Indians{''} as
   generic example of one of Twain's ``boy stories,{''}more interested in
   nostalgia than satire. But a case can be made that Tom Sawyer's
   Conspiracy as much belongs with works like Puddn'head Wilson and The
   Mysterious Stranger as part of a broader assessment of turn-of-the
   century modernity that Twain developed through his later years.},
