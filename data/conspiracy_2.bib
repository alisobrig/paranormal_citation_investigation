@article{ WOS:000419184000017,
Author = {Kovalev, Alexey Yu. and Muchnik, Viktor M. and Khazanov, Oleg V.},
Title = {THE THEORY OF JEWISH CONSPIRACY AND ITS TRANSFORMATION DURING TWO
   RUSSIAN REVOLUTIONS},
