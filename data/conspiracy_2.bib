@article{ WOS:000511959500004,
Author = {Polansky, David and Schillinger, Daniel},
Title = {With Steel or Poison: Machiavelli on Conspiracy},
Journal = {INTERPRETATION-A JOURNAL OF POLITICAL PHILOSOPHY},
Year = {2018},
Volume = {45},
Number = {1},
Pages = {67-86},
Month = {FAL},
Abstract = {Machiavelli's life, times, and writings converge on the topic of
   conspiracy. Yet his treatment of this topic is both more expansive and
   more complex than scholars have recognized. Attending to Discourses 3.6,
   among other central passages, we argue that the significance of
   conspiracy for Machiavelli's political thought lies in its connection to
   founding. In his analyses of historical conspiracies, Machiavelli shows
   what founding requires in practice; at the same time, the ubiquitous
   occurrence of conspiracies across historical and political contexts
   reveals the contested and contingent status of political order.
   Ultimately, Machiavelli depicts and analyzes conspiracies because he
   aims not only to investigate but also to produce new beginnings. To this
   end, Machiavelli deploys a novel rhetoric that challenges the reader to
   adopt a conspiratorial outlook, if not to become a conspirator himself.},
