@article{ WOS:000692756000029,
Author = {Belousov, Mikhail S. and Abdullaev, Yasyn S.},
Title = {THE MYTH ABOUT THE PAN-EUROPEAN CONSPIRACY: FROM THE LIBERAL TRIENNIUM
   TO THE DECEMBRIST REVOLT},
Journal = {HISTORIA CONSTITUCIONAL},
