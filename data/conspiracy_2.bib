@article{ WOS:000898337500001,
Author = {Logemann, Hannah Timna and Tomczyk, Samuel},
Title = {How Media Reports on COVID-19 Conspiracy Theories Impact Consensus
   Beliefs and Protective Action: A Randomized Controlled Online Trial},
Journal = {SCIENCE COMMUNICATION},
Year = {2023},
Volume = {45},
Number = {2},
Pages = {145-171},
Month = {APR},
Abstract = {This study examines the influence of news coverage on coronavirus
   disease (COVID)-related conspiracy theories on consensus perceptions
   regarding the seriousness of COVID-19 and its impact on attitudes and
   behaviors. In an online experiment, 395 participants either watched a
   report containing conspiracy theories, scientific facts, or information
   about a political summit, and they subsequently completed a
   questionnaire. Viewing reports on conspiracy theories lead to higher
   assessments of consensus compared with other reports. Perceived
   consensus correlated positively with attitudes toward COVID, which
   further correlated positively with behavior. The study shows that news
   reports can bias assessments of consensus, which has implications for
   public communication.},
