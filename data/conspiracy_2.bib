@article{ WOS:000798242200001,
Author = {Howard, Matt C. and Davis, Maggie M.},
Title = {The mediating role of vaccine hesitancy in the relations of COVID-19
   conspiracy beliefs and vaccination outcomes: Which dimensions matter?},
Journal = {JOURNAL OF HEALTH PSYCHOLOGY},
Year = {2023},
Volume = {28},
Number = {3},
Pages = {241-250},
Month = {MAR},
Abstract = {COVID-19 conspiracy beliefs have a powerful detrimental influence on
   COVID-19 vaccine perceptions and behaviors. We investigate an expanded
   range of outcomes for COVID-19 conspiracy beliefs, and we test which
   vaccine hesitancy dimensions mediate these relations. Our results show
   that COVID-19 conspiracy beliefs relate to COVID-19 vaccination
   willingness and receipt, flu vaccination willingness and receipt, as
   well as vaccine word-of-mouth. Many of these relations are mediated by
   vaccine hesitancy dimensions that represent perceptions that vaccines
   pose health risks as well as perceptions that vaccines are not needed
   because the respondent is healthy. Our discussion identifies directions
   for future research.},
