@article{ WOS:000679526800001,
Author = {Bleakley, Paul},
Title = {Panic, pizza and mainstreaming the alt-right: A social media analysis of
   Pizzagate and the rise of the QAnon conspiracy},
Journal = {CURRENT SOCIOLOGY},
Year = {2023},
Volume = {71},
Number = {3},
Pages = {509-525},
Month = {MAY},
Abstract = {The conspiracy theory known as `Pizzagate' gained a cult following on
   alt-right forums, ultimately prompting one believer to conduct a
   shooting on the pizzeria identified by online conspiracists. A thematic
   analysis of 767 tweets referencing Pizzagate selected from five key
