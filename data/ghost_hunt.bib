
@article{ WOS:000521750100006,
Author = {Georges, Fanny},
Title = {Ghost hunting on YouTube. Ethnographic and quali-quantitative approach
   to comments on videos},
Journal = {COMMUNIQUER},
Year = {2019},
Number = {27},
Pages = {99-122},
Abstract = {To what went earn contemporary culture he observed on the Internet? This
   article proposes to analyze the comments on YouTube of ghost hunting
   videos in a quali-quantitative approach, by mobilizing the theoretical
   framework of Yuri Lotman's cultural semiotics. Drawing on public data
   from Jour YouTube channels featuring paranormal investigations in
   haunted places, the analysis highlights the social and technical
   injunctions (social expectations, algoritlen), the common contributors
   to several chains, and the social function of the comments. The results
   show that the paranormal web is a place for the construction of an
   experiential and game-like updating of the ancestral culture of
   communication with spirits. Withdrawn from any quest for a single and
   indivisible truth, digital ghost hunters and their followers invest
   digital technologies as new mediums for a spiritual communication
   embodied as a concrete experience of physics and electromagnetic fields.},
