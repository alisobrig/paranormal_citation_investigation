@article{ WOS:000298988100006,
Author = {Gholizadeh, Shadi and Hook, Derek W.},
Title = {The Discursive Construction of the 1978-1979 Iranian Revolution in the
   Speeches of Ayatollah Khomeini},
Journal = {JOURNAL OF COMMUNITY \& APPLIED SOCIAL PSYCHOLOGY},
