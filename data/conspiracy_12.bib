Title = {HYDROSTATIC GAS CONSTRAINTS ON SUPERMASSIVE BLACK HOLE MASSES:
   IMPLICATIONS FOR HYDROSTATIC EQUILIBRIUM AND DYNAMICAL MODELING IN A
   SAMPLE OF EARLY-TYPE GALAXIES},
Journal = {ASTROPHYSICAL JOURNAL},
Year = {2009},
Volume = {703},
Number = {2},
