@article{ WOS:000248675000025,
Author = {Bania, T. M. and Balser, Dana S. and Rood, Robert T. and Wilson, T. L.
   and LaRocque, Jennifer M.},
Title = {<SUP>3</SUP>He in the Milky Way interstellar medium:: Ionization
   structure},
