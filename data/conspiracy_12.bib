Author = {Razzaque, Muhmooda Abdul and Haroon, Rukhsana Muhammad and Hafeez,
   Rubine and Hussain, Sabir and Razzaq, Abdul},
Title = {Factors affecting intentions of readiness and reluctance for COVID-19
   vaccination},
Journal = {RAWAL MEDICAL JOURNAL},
