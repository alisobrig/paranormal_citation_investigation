@article{ WOS:000468843000014,
Author = {Couceiro Rosillo, Sonia and Zamora Munoz, Maria Jose and Roman, Carlos
   J. van-der Hofstadt},
Title = {Areas of psychological exploration in palliative patients and their
   families: scientific basis and practical aspects},
