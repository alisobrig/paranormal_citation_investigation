@article{ WOS:000430244600028,
Author = {Khan, Muhammad Sajid and Zahoor, Muhammad Abrar and Naz, Humera},
Title = {The Xenophobia, Racial Identity and the Crisis of Federalism in East
   Pakistan},
Journal = {TARIH KULTUR VE SANAT ARASTIRMALARI DERGISI-JOURNAL OF HISTORY CULTURE
   AND ART RESEARCH},
