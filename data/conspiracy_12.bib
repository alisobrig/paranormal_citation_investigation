@article{ WOS:000517046800001,
Author = {Hughes, David A.},
Title = {9/11 Truth and the Silence of the IR Discipline},
Journal = {ALTERNATIVES},
Year = {2020},
Volume = {45},
Number = {2},
Pages = {55-82},
Month = {MAY},
Abstract = {International Relations (IR) scholars uncritically accept the official
   narrative regarding the events of 9/11 and refuse to examine the massive
   body of evidence generated by the 9/11 truth movement. Nevertheless, as
   calls for a new inquiry into the events of 9/11 continue to mount, with
   the International 9/11 Consensus Panel and World Trade Centre Building 7
