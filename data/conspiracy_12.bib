@article{ WOS:000608619000012,
Author = {Lutkenhaus, Roel O. and Jansz, Jeroen and Bouman, Martine P. A.},
Title = {Mapping the Dutch vaccination debate on Twitter: Identifying
   communities, narratives, and interactions},
