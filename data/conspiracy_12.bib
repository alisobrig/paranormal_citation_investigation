Author = {Cuesta-Cambra, Ubaldo and Martinez-Martinez, Luz and Nino-Gonzalez,
   Jose-Ignacio},
Title = {An analysis of pro-vaccine and anti-vaccine information on social
   networks and the Internet: Visual and emotional patterns},
