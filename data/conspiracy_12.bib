@article{ WOS:000210627800006,
Author = {Levy, David},
Title = {The Sad Tale of Fred Rose, Stalin's Man in the True North},
Journal = {INTERNATIONAL JOURNAL OF INTELLIGENCE AND COUNTERINTELLIGENCE},
Year = {2012},
Volume = {25},
Number = {2},
Pages = {350-366},
Abstract = {In the summer of 1943, Fred Rose, a candidate for the Labour Progressive
   Party, a Communist Party alias, was elected to the Canadian Parliament
   in a by-election in the federal constituency of Cartier, a working-class
   district in the city of Montreal. The margin of victory was thin.
   Cartier had a mixed Jewish and French Canadian demography. Whose votes
   won the day for Rose in 1943 remains unclear.(1) Re-elected in 1945, in
   September of that year a Soviet encryption specialist named Igor
   Gouzenko, notified that he was to be recalled to Moscow, fled the USSR's
   Ottawa embassy with dozens of documents he hoped to trade for asylum.
   The documents revealed that the embassy was a hothouse of espionage and
   identified the Member of Parliament (M.P.) Fred Rose as a major player
   in the scheme. A few months later, Rose was arrested, put on trial, and
   convicted of conspiracy to violate Canada's Official Secrets Act. The
   head of the Communist Party of Canada (CPC), Tim Buck, claimed the Party
   was completely innocent of any involvement in Rose's espionage activity.
   Following Rose's release from prison in 1953, the Party, eager to scrub
   clean its public face, drove him out of the country. Rose ended his days
   in Warsaw, Poland, an unhappy witness to the rise of Solidarity and the
   collapse of the Leninist vision he'd lived by.},
