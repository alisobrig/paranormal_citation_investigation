@article{ WOS:000311903000007,
Author = {Harding, Richard K.},
Title = {Samuel A. Mudd, MD, Physician-Farmer, University of Maryland School of
   Medicine Class of 1856},
Journal = {JOURNAL OF NERVOUS AND MENTAL DISEASE},
Year = {2012},
Volume = {200},
Number = {12},
Pages = {1043-1046},
Month = {DEC},
Abstract = {America is in the midst of experiencing the sesquicentennial of the
   Civil War. We do so with some ambivalence knowing that the war forged a
   great union and ended slavery but also caused the deaths of more than
   600,000 fellow citizens. Samuel A. Mudd, MD, University of Maryland
   School of Medicine class of 1856, was a man of this time. As a
   physician-farmer in Southern Maryland, he was a highly respected
   physician, a slave owner, and a devout citizen. The Civil War
   (1861-1865) would alter his life in ways few could have imagined. This
   article looks at his background, his education, his work as a
   physician-farmer, and his dramatic rise to national attention and
   infamy. Convicted by a military tribunal and imprisoned for his
   ``crimes,'' he was able to partially redeem himself using his medical
   skills and professionalism. Mudd was a man of his time. And what a time
   it was.},
