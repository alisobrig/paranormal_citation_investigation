Title = {RE-READING VSEVOLOD IVANOV'S STORY ``THE ARMORED TRAIN 14-69{''} AGAINST
   SIBERIAN PERIODICALS (1919): THE HISTORICAL COMMENT OF THE RUSSIAN
   ALLIES IN THE CIVIL WAR},
Journal = {STUDIA LITTERARUM},
Year = {2017},
