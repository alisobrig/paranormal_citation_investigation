@article{ WOS:000349907900014,
Author = {Somayaji, Darryl and Cloyes, Kristin Gates},
Title = {Cancer Fear and Fatalism How African American Participants Construct the
   Role of Research Subject in Relation to Clinical Cancer Research},
Journal = {CANCER NURSING},
