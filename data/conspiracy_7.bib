@article{ WOS:000215462800006,
Author = {Araujo, Silvio Vanderlei and de Sousa, Alisson Santos and Araujo,
   Emerson Wilian},
Title = {A multidimensional conspiracy around the software industry: arguments
   for intervention in high technology sectors},
Journal = {REVISTA BRASILEIRA DE INOVACAO},
Year = {2010},
Volume = {9},
Number = {2},
Pages = {291-321},
Month = {JUL-DEC},
Abstract = {This paper analyses the current debate about governmental intervention
   in high tech industries. On one hand, there are arguments against such
   interventions that consider the high tech industries as a capital
   intensive sector; instead of technological intensity, the capital
   intensity determines the value added. On the other hand, we could prove
   that there is at least one high tech industry, which is job intensive
   and non capital intensive, with high value added, that generates several
   positive externalities (the software industry). Such externalities are
   important arguments to support governmental intervention in high tech
   industries and cast doubts on the capital's value added argument.},
