@article{ WOS:000217411000011,
Author = {Heil, Johannes},
Title = {Conspiracy, Usury, and Hostility towards Jews Or: the Account of the
   Antichrist - A Sketch Wolfgang Benz on the seventieth Birthday},
Journal = {ASCHKENAS-ZEITSCHRIFT FUER GESCHICHTE UND KULTUR DER JUDEN},
Year = {2010},
Volume = {20},
Number = {2},
Pages = {395-413},
Month = {DEC},
Abstract = {Finance, interest- taking and > usury< played a central and sometimes
   even dominat role in the hostility towards Jews in the High and Late
   Middle Ages. The central role of the term usury in medieval anti- Jewish
   discourse is therefore a consequence of its compatibility with various
   issues and goals. The motifs of pawn broking and piety, for example,
   could be combined in accusations of host desecration. A closer look at
   the various sources including their extended discursive context shows
   that usury was or was supposed to be seen as a significant expression of
   Jewish nature and as a strategy for a conspiratorial realization of
   Jewish pursuit of power. The similarity to corresponding ideas of the
   19th and 20th centuries is obvious. While they are not simply identical
   due to the different social and political conditions in the Middle Ages
   and modern times, medieval ideas already clearly contain the structures
   of defamation strategies.},
