@article{ WOS:000208294500005,
Author = {Hudson, Rex A.},
Title = {Colombia's Palace of Justice Tragedy Revisited: A Critique of the
   Conspiracy Theory},
Journal = {TERRORISM AND POLITICAL VIOLENCE},
Year = {1995},
Volume = {7},
Number = {2},
Pages = {93-142},
Abstract = {This critique examines Ana Carrigan's The Palace of Justice: A Colombian
   Tragedy(1) by (1) taking issue with her contention that the Colombian
   Army massacred most of the hostages who died in the national courthouse
   in Bogota on 6-7 November 1985, including 11 members of the Supreme
   Court of Justice; (2) questioning her unsubstantiated claim that the
   Colombian government and military, in collaboration with the US Embassy
   in Bogota and the US Department of State, covered up the massacre by
   scripting an `official story', which labeled the hostage-taking
   operation by the 19th of April Movement (M-19) an act of narco-terrorism
   sponsored by the Medellin Cartel; and (3) assessing her reliance on
   anonymous sources and her disregard of the testimony of numerous
   credible survivors who witnessed the M-19 hostage massacres.},
Publisher = {ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD},
