@article{ WOS:000371228900007,
Author = {Goring, Paul},
Title = {THEATRICAL RIOTS AND CONSPIRACIES IN LONDON AND EDINBURGH: CHARLES
   MACKLIN, JAMES FENNELL AND THE RIGHTS OF ACTORS AND AUDIENCES},
Journal = {REVIEW OF ENGLISH STUDIES},
Year = {2016},
Volume = {67},
Number = {278},
Pages = {122-145},
Month = {FEB},
Abstract = {What constitutes a legitimate disruption of an actor's performance? In
   the 1770s this became a pressing question following the blighted attempt
   by Charles Macklin to stage Macbeth at Covent Garden, which resulted in
   one of theatre history's most drawn out battles between a performer and
   the public. Beginning with the hissing of the elderly actor in 1773, the
   conflict lasted nearly two years until it was finally settled in court
   before the Lord Chief Justice, Lord Mansfield. Around thirteen years
   after Macklin's court case, a far more obscure-but strikingly
   similar-controversy began at Edinburgh's Theatre Royal, concerning a
   performance by the aspiring actor James Fennell. This article presents
   previously unnoticed connections between the controversies and shows
   that an undated pamphlet, Case, Mr. Macklin, which has long been assumed
   to have been a 1775 intervention bolstering Macklin's cause, was
   actually published later in support of Fennell-even though it makes no
   mention of him. Positioning the pamphlet in the true context of its
   publication affords new insights into the legacy of Macklin's legal
   pursuit of his rights, and it allows an exploration of the processes
   through which the tense relationship between actors and factionalised
   audiences was renegotiated-involving theatre management, the press,
   public opinion, and the law-in both London and Edinburgh. The connection
   of the cases is furthermore revealing of the links between the separate
   legal systems of England and Scotland, in a period when rights within
   the theatre had become a matter for the law.},
