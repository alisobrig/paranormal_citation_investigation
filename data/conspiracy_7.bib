@article{ WOS:000889222300001,
Author = {Monheim, Chelsea L. and Himmelstein, Mary S.},
Title = {Democrats and independents stigmatize people with COVID-19 greater
   compared to Republicans},
Journal = {JOURNAL OF SOCIAL PSYCHOLOGY},
Year = {2023},
Volume = {163},
Number = {2},
Pages = {158-173},
Month = {MAR 4},
Abstract = {Messaging about COVID-19 was different across the political spectrum,
   which influenced differences in attitudes surrounding COVID-19. This
   study examined the political affiliation/ideology on COVID-19 stigma
   (blame, deservingness of help, negative emotionality) and two mediators
   of this relationship: conspiracy beliefs and anxiety about COVID-19.
   Participants answered questions about their political affiliation and
   ideology, attitudes toward people who have contracted COVID-19; and
   COVID-19 conspiracy beliefs and anxiety. Democrats and Independents
   indirectly stigmatized people with COVID-19 via increased COVID-19
   anxiety and fewer COVID-19 conspiracy beliefs relative to Republicans.
   Politicization can strongly impact stigma, and messaging could be
   harnessed as a stigma reduction tool.},
