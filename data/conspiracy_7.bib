@article{ WOS:000234785900012,
Author = {List, JA and Price, MK},
Title = {Conspiracies and secret price discounts in the marketplace: Evidence
   from field experiments},
Journal = {RAND JOURNAL OF ECONOMICS},
Year = {2005},
Volume = {36},
Number = {3},
Pages = {700-717},
Month = {FAL},
Abstract = {We explore collusion by using the tools of experimental economics in a
   naturally occurring marketplace. We report that competitive price theory
   adequately organizes data in multilateral decentralized bargaining
   markets without conspiratorial opportunities. When conspiratorial
   opportunities are allowed and contract prices are perfectly observed,
   prices (quantities) are considerably above (below) competitive levels.
