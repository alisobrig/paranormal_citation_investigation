@article{ WOS:000264299700004,
Author = {Mason, Laura},
Title = {After the Conspiracy: the Directory, the press, and the Affair of the
   Equals},
Journal = {ANNALES HISTORIQUES DE LA REVOLUTION FRANCAISE},
Year = {2008},
Number = {354},
Pages = {77+},
Month = {OCT-DEC},
Abstract = {The arrest of Gracchus Babeuf and the Equals in Floreal year IV
   integrated Babeuf into directorial political culture by transforming him
   from popular activist to press icon. Over the next year, the Directory
   and a newly-vibrant periodical press would publicize Babeuf's name well
   beyond anything he had yet achieved. Democratic newspapers, in
   particular, would cease to represent Babeuf as a agitator or misguided
   critic, coming instead to celebrate him as republican conscience and
   martyr. As the affair forged Babeuf's reputation, it weakened the
   Directory by polarizing right and left, renewing political hatreds the
   new regime was meant to heal.},
