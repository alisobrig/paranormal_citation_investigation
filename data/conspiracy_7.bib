@article{ WOS:000310680800004,
Author = {Dyck, Joshua J. and Pearson-Merkowitz, Shanna},
Title = {The Conspiracy of Silence: Context and Voting on Gay Marriage Ballot
   Measures},
Journal = {POLITICAL RESEARCH QUARTERLY},
Year = {2012},
Volume = {65},
Number = {4},
Pages = {745-757},
Month = {DEC},
Abstract = {Gay marriage bans have received widespread support in the past decade in
   states across America. The authors evaluate the extent to which context
   mitigates or exacerbates support for gay rights. Their question is
   grounded in the debate over whether exposure to out-groups creates
   acceptance through contact or whether exposure induces hostility through
   threat. Using data from twelve marriage ban elections, the authors find
   conditional support for the threat hypothesis. While the presence of a
   gay population is unimportant to the majority of the population,
   evangelical Christians who live proximate to larger gay populations are
   more likely to support bans on gay marriage.},
