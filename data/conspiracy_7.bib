@article{ WOS:000371114800008,
Author = {Fisher, Louis},
Title = {<i>The Law</i> Jefferson and the Burr Conspiracy: Executive Power
   against the Law},
Journal = {PRESIDENTIAL STUDIES QUARTERLY},
