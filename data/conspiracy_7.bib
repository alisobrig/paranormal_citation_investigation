@article{ WOS:000368403000007,
Author = {Fritsche, Johannes},
Title = {Nazism, Anti-Semitism and Philosophy for Heidegger and Scheler to
   Trawnys <i>Heidegger</i> and the <i>Myth</i> of the Jewish <i>World
   Conspiracy</i>},
Journal = {DEUTSCHE ZEITSCHRIFT FUR PHILOSOPHIE},
Year = {2015},
Volume = {63},
Number = {5},
Pages = {913-940},
Month = {OCT},
Abstract = {According to Trawny, Heidegger's Black Notebooks show that his thinking
   could be ``contaminated{''} by National Socialism and anti-Semitism only
   between 1931 and 1944/1945. However, in this paper it is argued that
   already in Being and Time (1927) Heidegger had made a case for National
   Socialism, which he discovered in 1938 - the `true' National Socialism
   -, and that Trawny's main criterion is false. Heidegger's case is
   compared with Max Scheler, who, because of Hitler, turned from the right
   to the centre. In addition, alternatives to Trawny's detailed
   interpretations of three of Heidegger's anti-Semitic remarks are
   offered, and the anti-Semitic aspects of Heidegger's history of Being
   are presented.},
