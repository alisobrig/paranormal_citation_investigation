@article{ WOS:000939492200007,
Author = {Fontana, Vanessa Furtado},
Title = {Husserl and the reflection of the crisis of Philosophy in the crisis of
   humanity},
Journal = {ARGUMENTOS-REVISTA DE FILOSOFIA},
