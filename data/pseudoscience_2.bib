@article{ WOS:000653281400001,
Author = {Garrett, Bernie},
Title = {Divinity in nursing: The complexities of adopting a spiritual basis for
   care},
Journal = {NURSING PHILOSOPHY},
Year = {2021},
Volume = {22},
Number = {3, SI},
Month = {JUL},
Abstract = {In this paper, the historical alignment of nursing with divinity-based
   perspectives and modern New Age nursing theories are explored. The
   nature of divinity in nursing is examined, together with the
   complexities and issues that arise in adopting a spiritual basis for
   care. The work of the key theorists in this area (Rogers, Newman, Parse,
   Watson, Dossey) is reconsidered and fundamental epistemological problems
   inherent in this approach reviewed. Specific concerns with the
   interpretation of holistic care, adoption of doxastic logic, faith-based
   rationales, influence of Caucasian Judeo-Christian and New Age values,
   misappropriation of science, use of pseudoscience and development of
   divinity as social consumer product are discussed. Practical problems
   with using a spiritual basis in contemporary nursing practice are also
   explored and the alignment of divinity-based nursing theories with the
   modern antiscience movement is examined. Overall, the re-emergence of
   divinity-based nursing and spiritual basis for care may be problematic,
   and would seem to counter the development of an inclusive profession.
   Additionally, the alignment of nursing with antiscience movements
   rejecting biomedicine has significant public health implications. This
   is argued as a regressive step for the advancement of the profession.},
