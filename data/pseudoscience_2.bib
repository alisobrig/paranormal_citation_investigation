@article{ WOS:000174852500001,
Author = {Hayes, KJ},
Title = {Visual culture and the word in Edgar Allan Poe's The `Man of the Crowd'},
Journal = {NINETEENTH-CENTURY LITERATURE},
Year = {2002},
Volume = {56},
Number = {4},
Pages = {445-465},
Month = {MAR},
Abstract = {As the nameless narrator of Edgar Allans Poe's ``The Man of the
   Crowd{''} (1840) begins to relate his personal experience in a
   coffeehouse and on the streets of London, he has complete confidence in
   his ability to read the crowd on the basis of external signs. By having
   narrator alternate reading his newspaper with observing the crowd, Poe
   parallels the two activities and emphasizes their similarity. Comparing
   the act of gazing through a window with reading a written text, Poe
   compared the acts of seeing and reading. In part inspired by the
   pseudosciences of physiognomy anf phrenology, the ability to read the
   character of people in a crowd was also a part of the increasingly
   numerous written texts that were starting to appear as part of the
   modern urban landscape. As nations approached near universal literacy,
   their cities began to contain more and more writing. Observing the
   streets, both literally and figuratively , was becoming a matter of
   reading. The contemporary figure of the sandwichman reinforced the
   similarity between written texts and reading a period in the
   relationship between word and image. Centuries before, the written word
   had taken precedence over the visual image, but in this new period the
   written word was beginning to take on the qualities of an image.},
