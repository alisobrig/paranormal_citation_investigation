@article{ WOS:000539920000006,
Author = {Cozart, Dan},
Title = {<i>Peruanidad</i> and blackness in national and local perspectives:
   popular literature and racial science},
Journal = {LATIN AMERICAN AND CARIBBEAN ETHNIC STUDIES},
Year = {2019},
Volume = {14},
Number = {2},
Pages = {194-213},
Abstract = {This article examines the constructions of ethno-racial identities in
   the debates and projects of Peruvian nation building during the late
   nineteenth and early twentieth centuries. The conflicting writings of
   Lima's elite authors Ricardo and Clemente Palma contrast with literature
   from the northern region of Piura, exposing differing regional and local
   conceptions of peruanidad as well as distinct interpretations of
   colonial history in the shaping of a national identity. Whereas Ricardo
   Palma's tradiciones promoted a somewhat inclusive vision of Peruvian
   national identity, his son Clemente's bachelor's thesis reveals a social
   structure defined by an emerging vocabulary that established ethnic and
   racial boundaries. These perspectives, however, were further challenged
   by Piuran poet and novelist Enrique Lopez Albujar, whose work Matalache
   asserted the significance of Afro-Peruvians in the nation's history and
   identity. Discursive shifts evident in Clemente Palma's writings bear
   similarities to Peru's leading eugenicist of the 1930s, Roberto MacLean
   Estrenos, demonstrating the interplay between popular culture and the
   pseudoscience of race. The analysis shows similarities in discourses of
   race and nation across the Americas while highlighting diverse
   perspectives that were ultimately constrained by a dominant discourse of
   mestizaje.},
