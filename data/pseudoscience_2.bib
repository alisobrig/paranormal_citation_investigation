@article{ WOS:000471782800001,
Author = {Cano-Oron, Lorena and Mendoza-Poudereux, Isabel and Moreno-Castro,
   Carolina},
Title = {The rise of skepticism in Spanish political and digital media contexts},
Journal = {JCOM-JOURNAL OF SCIENCE COMMUNICATION},
Year = {2019},
Volume = {18},
Number = {3, SI},
Abstract = {Currently in Spain, there is a political and social debate over the use
   and sale of homeopathic products, which is promoted mainly by the
   skeptical movement. For the first time, this issue has become
   significant in political discourse. This study analyzes the role that
   homeopathy-related stories are playing in that political debate. We
   analyzed the viewpoints of headlines between 2015 and 2017 in eight
   digital dailies (n = 1,683), which published over 30 stories on
   homeopathy during the three-year study period. The results indicated
   that the stance on therapy's lack of scientific evidence gained ground
   during the period studied.},
