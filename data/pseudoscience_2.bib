@article{ WOS:000339727600024,
Author = {Matute, Helena and Blanco, Fernando},
Title = {Reducing the illusion of control when an action is followed by an
   undesired outcome},
Journal = {PSYCHONOMIC BULLETIN \& REVIEW},
